; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_sincos_private_v3float.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %sincos_v3float "sincos_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
       %v3float_ptr_t = OpTypePointer Function %v3float_t
         %sincos_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.05
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
      %sincos_v3float = OpFunction %void_t None %sincos_fn_t
; CHECK: define spir_kernel void @sincos_v3float()
                   %2 = OpLabel
                   %3 = OpVariable %v3float_ptr_t Function
; CHECK: [[cosval:%[a-zA-Z0-9]+]] = alloca <3 x float>
                   %4 = OpExtInst %v3float_t %1 sincos %v3float_1 %3
; CHECK-GE15: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z6sincosDv3_fPS_(<3 x float> <float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000>, ptr [[cosval]])
; CHECK-LT15: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z6sincosDv3_fPS_(<3 x float> <float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000>, <3 x float>* [[cosval]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
