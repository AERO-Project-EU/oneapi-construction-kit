; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_fract_global_float.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fract_float "fract_float"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
         %float_ptr_t = OpTypePointer CrossWorkgroup %float_t
          %fract_fn_t = OpTypeFunction %void_t %float_ptr_t
             %float_1 = OpConstant %float_t 1.4

; All functions
         %fract_float = OpFunction %void_t None %fract_fn_t
              %in_ptr = OpFunctionParameter %float_ptr_t
; CHECK-GE15: define spir_kernel void @fract_float(ptr addrspace(1) %in_ptr)
; CHECK-LT15: define spir_kernel void @fract_float(float addrspace(1)* %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 fract %float_1 %in_ptr
; CHECK-GE15: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z5fractfPU3AS1f(float 0x3FF6666660000000, ptr addrspace(1) %in_ptr)
; CHECK-LT15: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z5fractfPU3AS1f(float 0x3FF6666660000000, float addrspace(1)* %in_ptr)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
