; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_s_add_sat_v8i16.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int16
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_add_sat_v8i16 "s_add_sat_v8i16"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i16_t = OpTypeInt 16 1
             %v8i16_t = OpTypeVector %i16_t 8
      %s_add_sat_fn_t = OpTypeFunction %void_t
               %i16_1 = OpConstant %i16_t 1
               %i16_2 = OpConstant %i16_t 2
             %v8i16_1 = OpConstantComposite %v8i16_t %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1
             %v8i16_2 = OpConstantComposite %v8i16_t %i16_2 %i16_2 %i16_2 %i16_2 %i16_2 %i16_2 %i16_2 %i16_2

; All functions
     %s_add_sat_v8i16 = OpFunction %void_t None %s_add_sat_fn_t
; CHECK: define spir_kernel void @s_add_sat_v8i16()
                   %2 = OpLabel
                   %3 = OpExtInst %v8i16_t %1 s_add_sat %v8i16_1 %v8i16_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x i16> @_Z7add_satDv8_sS_(<8 x i16> <i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1>, <8 x i16> <i16 2, i16 2, i16 2, i16 2, i16 2, i16 2, i16 2, i16 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
