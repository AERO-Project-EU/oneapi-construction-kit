; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a Vulkan -c Float64 -c Int64 %p/op_constant_null_float.spv -o %t
; RUN: %filecheck < %t %t.pp
            OpCapability Shader
            OpCapability Float64
            OpCapability Int64
       %1 = OpExtInstImport "GLSL.std.450"
            OpMemoryModel Logical GLSL450
            OpEntryPoint GLCompute %main "main"
            OpExecutionMode %main LocalSize 1 1 1
            OpSource GLSL 450
            OpName %main "main"
            OpName %a "a"
       %void = OpTypeVoid
          %5 = OpTypeFunction %void
        %float = OpTypeFloat 32
    %ptr_float = OpTypePointer Function %float
          %8 = OpConstantNull %float
       %main = OpFunction %void None %5
          %9 = OpLabel
          %a = OpVariable %ptr_float Function
               OpStore %a %8
               OpReturn
               OpFunctionEnd
; CHECK: ; ModuleID = '{{.*}}'
; CHECK: define spir_kernel void @main()
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = alloca float
; CHECK-GE15: store float 0.000000e+00, ptr {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK-LT15: store float 0.000000e+00, float* {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK: ret void
