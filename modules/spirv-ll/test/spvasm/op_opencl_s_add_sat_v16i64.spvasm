; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_s_add_sat_v16i64.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_add_sat_v16i64 "s_add_sat_v16i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 1
            %v16i64_t = OpTypeVector %i64_t 16
      %s_add_sat_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1
               %i64_2 = OpConstant %i64_t 2
            %v16i64_1 = OpConstantComposite %v16i64_t %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1
            %v16i64_2 = OpConstantComposite %v16i64_t %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2

; All functions
    %s_add_sat_v16i64 = OpFunction %void_t None %s_add_sat_fn_t
; CHECK: define spir_kernel void @s_add_sat_v16i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v16i64_t %1 s_add_sat %v16i64_1 %v16i64_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x i64> @_Z7add_satDv16_lS_(<16 x i64> <i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1>, <16 x i64> <i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
