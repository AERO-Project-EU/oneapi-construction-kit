; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_floor_v8float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %floor_v8float "floor_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
          %floor_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1

; All functions
       %floor_v8float = OpFunction %void_t None %floor_fn_t
; CHECK: define spir_kernel void @floor_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 floor %v8float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z5floorDv8_f(<8 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
