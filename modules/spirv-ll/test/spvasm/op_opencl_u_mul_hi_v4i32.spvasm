; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_mul_hi_v4i32.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_mul_hi_v4i32 "u_mul_hi_v4i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
             %v4i32_t = OpTypeVector %i32_t 4
       %u_mul_hi_fn_t = OpTypeFunction %void_t
               %i32_2 = OpConstant %i32_t 2
               %i32_3 = OpConstant %i32_t 3
             %v4i32_2 = OpConstantComposite %v4i32_t %i32_2 %i32_2 %i32_2 %i32_2
             %v4i32_3 = OpConstantComposite %v4i32_t %i32_3 %i32_3 %i32_3 %i32_3

; All functions
      %u_mul_hi_v4i32 = OpFunction %void_t None %u_mul_hi_fn_t
; CHECK: define spir_kernel void @u_mul_hi_v4i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v4i32_t %1 u_mul_hi %v4i32_2 %v4i32_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x i32> @_Z6mul_hiDv4_jS_(<4 x i32> <i32 2, i32 2, i32 2, i32 2>, <4 x i32> <i32 3, i32 3, i32 3, i32 3>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
