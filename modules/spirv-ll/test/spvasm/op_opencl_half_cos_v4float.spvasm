; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_half_cos_v4float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_cos_v4float "half_cos_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
       %half_cos_fn_t = OpTypeFunction %void_t
             %float_3 = OpConstant %float_t 3.14
           %v4float_3 = OpConstantComposite %v4float_t %float_3 %float_3 %float_3 %float_3

; All functions
    %half_cos_v4float = OpFunction %void_t None %half_cos_fn_t
; CHECK: define spir_kernel void @half_cos_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 half_cos %v4float_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z8half_cosDv4_f(<4 x float> <float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
