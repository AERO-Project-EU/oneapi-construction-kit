; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_shuffle_v8i64.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_shuffle_v8i64 "u_shuffle_v8i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
             %v8i64_t = OpTypeVector %i64_t 8
      %u_shuffle_fn_t = OpTypeFunction %void_t
               %i64_3 = OpConstant %i64_t 3
               %i64_4 = OpConstant %i64_t 4
             %v8i64_3 = OpConstantComposite %v8i64_t %i64_3 %i64_3 %i64_3 %i64_3 %i64_3 %i64_3 %i64_3 %i64_3
             %v8i64_4 = OpConstantComposite %v8i64_t %i64_4 %i64_4 %i64_4 %i64_4 %i64_4 %i64_4 %i64_4 %i64_4

; All functions
     %u_shuffle_v8i64 = OpFunction %void_t None %u_shuffle_fn_t
; CHECK: define spir_kernel void @u_shuffle_v8i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v8i64_t %1 shuffle %v8i64_3 %v8i64_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x i64> @_Z7shuffleDv8_mS_(<8 x i64> <i64 3, i64 3, i64 3, i64 3, i64 3, i64 3, i64 3, i64 3>, <8 x i64> <i64 4, i64 4, i64 4, i64 4, i64 4, i64 4, i64 4, i64 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
