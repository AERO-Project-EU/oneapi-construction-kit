; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %p/op_opencl_bitselect_double.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %bitselect_double "bitselect_double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
      %bitselect_fn_t = OpTypeFunction %void_t
            %double_1 = OpConstant %double_t 1.0
            %double_2 = OpConstant %double_t 2.0
            %double_3 = OpConstant %double_t 3.0

; All functions
    %bitselect_double = OpFunction %void_t None %bitselect_fn_t
; CHECK: define spir_kernel void @bitselect_double()
                   %2 = OpLabel
                   %3 = OpExtInst %double_t %1 bitselect %double_1 %double_2 %double_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func double @_Z9bitselectddd(double 1.000000e+00, double 2.000000e+00, double 3.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
