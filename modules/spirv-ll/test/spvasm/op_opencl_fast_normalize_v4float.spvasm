; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_fast_normalize_v4float.spv -o %t
; RUN: %filecheck < %t %s
                          OpCapability Kernel
                          OpCapability Addresses
                     %1 = OpExtInstImport "OpenCL.std"
                          OpMemoryModel Physical64 OpenCL
                          OpEntryPoint Kernel %fast_normalize_v4float "fast_normalize_v4float"
                          OpSource OpenCL_C 102000

; All types, variables and constants
                %void_t = OpTypeVoid
               %float_t = OpTypeFloat 32
             %v4float_t = OpTypeVector %float_t 4
   %fast_normalize_fn_t = OpTypeFunction %void_t
               %float_5 = OpConstant %float_t 5.0
             %v4float_5 = OpConstantComposite %v4float_t %float_5 %float_5 %float_5 %float_5

; All functions
%fast_normalize_v4float = OpFunction %void_t None %fast_normalize_fn_t
; CHECK: define spir_kernel void @fast_normalize_v4float()
                     %2 = OpLabel
                     %3 = OpExtInst %v4float_t %1 fast_normalize %v4float_5
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z14fast_normalizeDv4_f(<4 x float> <float 5.000000e+00, float 5.000000e+00, float 5.000000e+00, float 5.000000e+00>)
                          OpReturn
; CHECK: ret void
                          OpFunctionEnd
; CHECK: }
