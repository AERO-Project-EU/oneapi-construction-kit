; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_mad_hi_v2i64.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_mad_hi_v2i64 "u_mad_hi_v2i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
             %v2i64_t = OpTypeVector %i64_t 2
       %u_mad_hi_fn_t = OpTypeFunction %void_t
               %i64_2 = OpConstant %i64_t 2
               %i64_3 = OpConstant %i64_t 3
               %i64_4 = OpConstant %i64_t 4
             %v2i64_2 = OpConstantComposite %v2i64_t %i64_2 %i64_2
             %v2i64_3 = OpConstantComposite %v2i64_t %i64_3 %i64_3
             %v2i64_4 = OpConstantComposite %v2i64_t %i64_4 %i64_4

; All functions
      %u_mad_hi_v2i64 = OpFunction %void_t None %u_mad_hi_fn_t
; CHECK: define spir_kernel void @u_mad_hi_v2i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v2i64_t %1 u_mad_hi %v2i64_2 %v2i64_3 %v2i64_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x i64> @_Z6mad_hiDv2_mS_S_(<2 x i64> <i64 2, i64 2>, <2 x i64> <i64 3, i64 3>, <2 x i64> <i64 4, i64 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
