; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_erf_v2float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %erf_v2float "erf_v2float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v2float_t = OpTypeVector %float_t 2
            %erf_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
           %v2float_1 = OpConstantComposite %v2float_t %float_1 %float_1

; All functions
         %erf_v2float = OpFunction %void_t None %erf_fn_t
; CHECK: define spir_kernel void @erf_v2float()
                   %2 = OpLabel
                   %3 = OpExtInst %v2float_t %1 erf %v2float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x float> @_Z3erfDv2_f(<2 x float> <float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
