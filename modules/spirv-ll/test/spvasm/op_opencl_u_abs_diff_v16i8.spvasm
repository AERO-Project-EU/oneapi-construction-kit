; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_abs_diff_v16i8.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int8
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_abs_diff_v16i8 "u_abs_diff_v16i8"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
                %i8_t = OpTypeInt 8 0
             %v16i8_t = OpTypeVector %i8_t 16
     %u_abs_diff_fn_t = OpTypeFunction %void_t
                %i8_2 = OpConstant %i8_t 2
                %i8_3 = OpConstant %i8_t 3
             %v16i8_2 = OpConstantComposite %v16i8_t %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2
             %v16i8_3 = OpConstantComposite %v16i8_t %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3 %i8_3

; All functions
    %u_abs_diff_v16i8 = OpFunction %void_t None %u_abs_diff_fn_t
; CHECK: define spir_kernel void @u_abs_diff_v16i8()
                   %2 = OpLabel
                   %3 = OpExtInst %v16i8_t %1 u_abs_diff %v16i8_2 %v16i8_3
