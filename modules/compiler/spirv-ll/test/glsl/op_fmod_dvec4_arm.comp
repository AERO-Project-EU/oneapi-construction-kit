void main() {
// CHECK: define spir_kernel void @main() {
// CHECK:  {{%[a-zA-Z0-9_]+}} = alloca <4 x double>
// CHECK:  {{%[a-zA-Z0-9_]+}} = alloca <4 x double>
// CHECK:  {{%[a-zA-Z0-9_]+}} = alloca <4 x double>
  dvec4 x = dvec4(-42.42, -42.42, -42.42, -42.42);
