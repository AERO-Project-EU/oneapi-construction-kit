; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_normalize_float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %normalize_float "normalize_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
      %normalize_fn_t = OpTypeFunction %void_t
             %float_5 = OpConstant %float_t 5.0

; All functions
     %normalize_float = OpFunction %void_t None %normalize_fn_t
; CHECK: define spir_kernel void @normalize_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 normalize %float_5
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z9normalizef(float 5.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
