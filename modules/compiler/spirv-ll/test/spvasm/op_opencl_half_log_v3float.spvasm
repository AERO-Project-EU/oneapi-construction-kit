; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_log_v3float "half_log_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
       %half_log_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
    %half_log_v3float = OpFunction %void_t None %half_log_fn_t
; CHECK: define spir_kernel void @half_log_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 half_log %v3float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z8half_logDv3_f(<3 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
