; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_modf_private_v3float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %modf_v3float "modf_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
       %v3float_ptr_t = OpTypePointer Function %v3float_t
           %modf_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
        %modf_v3float = OpFunction %void_t None %modf_fn_t
; CHECK: define spir_kernel void @modf_v3float()
                   %2 = OpLabel
                   %3 = OpVariable %v3float_ptr_t Function
; CHECK: [[iptr:%[a-zA-Z0-9]+]] = alloca <3 x float>
                   %4 = OpExtInst %v3float_t %1 modf %v3float_1 %3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z4modfDv3_fPS_(<3 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr [[iptr]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
