; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_fmod_float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fmod_float "fmod_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %fmod_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
             %float_2 = OpConstant %float_t 2

; All functions
          %fmod_float = OpFunction %void_t None %fmod_fn_t
; CHECK: define spir_kernel void @fmod_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 fmod %float_2 %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z4fmodff(float 2.000000e+00, float 1.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
