; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_min_i32.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_min_i32 "u_min_i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
          %u_min_fn_t = OpTypeFunction %void_t
               %i32_1 = OpConstant %i32_t 1
               %i32_2 = OpConstant %i32_t 2

; All functions
           %u_min_i32 = OpFunction %void_t None %u_min_fn_t
; CHECK: define spir_kernel void @u_min_i32()
                   %2 = OpLabel
                   %3 = OpExtInst %i32_t %1 u_min %i32_1 %i32_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func i32 @_Z3minjj(i32 1, i32 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
