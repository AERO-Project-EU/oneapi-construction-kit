; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_clz_i64 "s_clz_i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 1
          %s_clz_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1

; All functions
           %s_clz_i64 = OpFunction %void_t None %s_clz_fn_t
; CHECK: define spir_kernel void @s_clz_i64()
                   %2 = OpLabel
                   %3 = OpExtInst %i64_t %1 clz %i64_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func i64 @_Z3clzl(i64 1)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
