; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_log1p_float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %log1p_float "log1p_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
          %log1p_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4

; All functions
         %log1p_float = OpFunction %void_t None %log1p_fn_t
; CHECK: define spir_kernel void @log1p_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 log1p %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z5log1pf(float 0x3FF6666660000000)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
