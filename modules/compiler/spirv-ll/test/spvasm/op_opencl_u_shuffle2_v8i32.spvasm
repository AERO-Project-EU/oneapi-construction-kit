; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_shuffle2_v8i32 "u_shuffle2_v8i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
             %v8i32_t = OpTypeVector %i32_t 8
     %u_shuffle2_fn_t = OpTypeFunction %void_t
               %i32_3 = OpConstant %i32_t 3
               %i32_4 = OpConstant %i32_t 4
               %i32_5 = OpConstant %i32_t 5
             %v8i32_3 = OpConstantComposite %v8i32_t %i32_3 %i32_3 %i32_3 %i32_3 %i32_3 %i32_3 %i32_3 %i32_3
             %v8i32_4 = OpConstantComposite %v8i32_t %i32_4 %i32_4 %i32_4 %i32_4 %i32_4 %i32_4 %i32_4 %i32_4
             %v8i32_5 = OpConstantComposite %v8i32_t %i32_5 %i32_5 %i32_5 %i32_5 %i32_5 %i32_5 %i32_5 %i32_5

; All functions
    %u_shuffle2_v8i32 = OpFunction %void_t None %u_shuffle2_fn_t
; CHECK: define spir_kernel void @u_shuffle2_v8i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v8i32_t %1 shuffle2 %v8i32_3 %v8i32_4 %v8i32_5
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x i32> @_Z8shuffle2Dv8_jS_S_(<8 x i32> <i32 3, i32 3, i32 3, i32 3, i32 3, i32 3, i32 3, i32 3>, <8 x i32> <i32 4, i32 4, i32 4, i32 4, i32 4, i32 4, i32 4, i32 4>, <8 x i32> <i32 5, i32 5, i32 5, i32 5, i32 5, i32 5, i32 5, i32 5>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
