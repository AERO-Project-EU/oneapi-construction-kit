; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_maxmag_float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %maxmag_float "maxmag_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
         %maxmag_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.2
             %float_2 = OpConstant %float_t -2.1

; All functions
        %maxmag_float = OpFunction %void_t None %maxmag_fn_t
; CHECK: define spir_kernel void @maxmag_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 maxmag %float_2 %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z6maxmagff(float 0xC000CCCCC0000000, float 0x3FF3333340000000)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
