; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fabs_float "fabs_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %fabs_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1

; All functions
          %fabs_float = OpFunction %void_t None %fabs_fn_t
; CHECK: define spir_kernel void @fabs_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 fabs %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z4fabsf(float 1.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
