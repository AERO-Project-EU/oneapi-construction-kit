; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %floor_float "floor_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
          %floor_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4

; All functions
         %floor_float = OpFunction %void_t None %floor_fn_t
; CHECK: define spir_kernel void @floor_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 floor %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z5floorf(float 0x3FF6666660000000)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
