; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_fract_local_v4float.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fract_v4float "fract_v4float"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
       %v4float_ptr_t = OpTypePointer Workgroup %v4float_t
          %fract_fn_t = OpTypeFunction %void_t %v4float_ptr_t
             %float_1 = OpConstant %float_t 1.4
           %v4float_1 = OpConstantComposite %v4float_t %float_1 %float_1 %float_1 %float_1

; All functions
       %fract_v4float = OpFunction %void_t None %fract_fn_t
              %in_ptr = OpFunctionParameter %v4float_ptr_t
; CHECK-GE15: define spir_kernel void @fract_v4float(ptr addrspace(3) %in_ptr)
; CHECK-LT15: define spir_kernel void @fract_v4float(<4 x float> addrspace(3)* %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 fract %v4float_1 %in_ptr
; CHECK-GE15: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z5fractDv4_fPU3AS3S_(<4 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr addrspace(3) %in_ptr)
; CHECK-LT15: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z5fractDv4_fPU3AS3S_(<4 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, <4 x float> addrspace(3)* %in_ptr)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
