; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_cosh_v8float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"                        
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %cosh_v8float "cosh_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
           %cosh_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1

; All functions
        %cosh_v8float = OpFunction %void_t None %cosh_fn_t
; CHECK: define spir_kernel void @cosh_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 cosh %v8float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z4coshDv8_f(<8 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
