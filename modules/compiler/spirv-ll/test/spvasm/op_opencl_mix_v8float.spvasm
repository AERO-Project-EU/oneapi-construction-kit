; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %mix_v8float "mix_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
            %mix_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.1
             %float_2 = OpConstant %float_t 2.2
             %float_3 = OpConstant %float_t 3.3
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1
           %v8float_2 = OpConstantComposite %v8float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2
           %v8float_3 = OpConstantComposite %v8float_t %float_3 %float_3 %float_3 %float_3 %float_3 %float_3 %float_3 %float_3

; All functions
         %mix_v8float = OpFunction %void_t None %mix_fn_t
; CHECK: define spir_kernel void @mix_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 mix %v8float_1 %v8float_2 %v8float_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z3mixDv8_fS_S_(<8 x float> <float 0x3FF19999A0000000, float 0x3FF19999A0000000, float 0x3FF19999A0000000, float 0x3FF19999A0000000, float 0x3FF19999A0000000, float 0x3FF19999A0000000, float 0x3FF19999A0000000, float 0x3FF19999A0000000>, <8 x float> <float 0x40019999A0000000, float 0x40019999A0000000, float 0x40019999A0000000, float 0x40019999A0000000, float 0x40019999A0000000, float 0x40019999A0000000, float 0x40019999A0000000, float 0x40019999A0000000>, <8 x float> <float 0x400A666660000000, float 0x400A666660000000, float 0x400A666660000000, float 0x400A666660000000, float 0x400A666660000000, float 0x400A666660000000, float 0x400A666660000000, float 0x400A666660000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
