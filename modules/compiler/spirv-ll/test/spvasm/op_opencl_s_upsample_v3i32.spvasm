; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_s_upsample_v3i32.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_upsample_v3i32 "s_upsample_v3i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
              %ui16_t = OpTypeInt 16 0
               %i32_t = OpTypeInt 32 0
            %v3ui16_t = OpTypeVector %ui16_t 3
             %v3i32_t = OpTypeVector %i32_t 3
     %s_upsample_fn_t = OpTypeFunction %void_t
               %i16_3 = OpConstant %ui16_t 3
              %ui16_4 = OpConstant %ui16_t 4
             %v3i16_3 = OpConstantComposite %v3ui16_t %i16_3 %i16_3 %i16_3
            %v3ui16_4 = OpConstantComposite %v3ui16_t %ui16_4 %ui16_4 %ui16_4

; All functions
    %s_upsample_v3i32 = OpFunction %void_t None %s_upsample_fn_t
; CHECK: define spir_kernel void @s_upsample_v3i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v3i32_t %1 s_upsample %v3i16_3 %v3ui16_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x i32> @_Z8upsampleDv3_sDv3_t(<3 x i16> <i16 3, i16 3, i16 3>, <3 x i16> <i16 4, i16 4, i16 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
