; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %lgamma_v8float "lgamma_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
         %lgamma_fn_t = OpTypeFunction %void_t
             %float_3 = OpConstant %float_t 3.14
           %v8float_3 = OpConstantComposite %v8float_t %float_3 %float_3 %float_3 %float_3 %float_3 %float_3 %float_3 %float_3

; All functions
      %lgamma_v8float = OpFunction %void_t None %lgamma_fn_t
; CHECK: define spir_kernel void @lgamma_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 lgamma %v8float_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z6lgammaDv8_f(<8 x float> <float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000, float 0x40091EB860000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
