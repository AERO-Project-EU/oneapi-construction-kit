; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %remquo_v3float "remquo_v3float"
                        OpSource OpenCL_C 102000
                        OpName %in_quo "in_quo"

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
             %v3int_t = OpTypeVector %int_t 3
           %v3float_t = OpTypeVector %float_t 3
         %v3int_ptr_t = OpTypePointer Workgroup %v3int_t
         %remquo_fn_t = OpTypeFunction %void_t %v3int_ptr_t
             %float_1 = OpConstant %float_t 1.5
             %float_2 = OpConstant %float_t 2.0
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1
           %v3float_2 = OpConstantComposite %v3float_t %float_2 %float_2 %float_2

; All functions
      %remquo_v3float = OpFunction %void_t None %remquo_fn_t
              %in_quo = OpFunctionParameter %v3int_ptr_t
; CHECK: define spir_kernel void @remquo_v3float(ptr addrspace(3) %in_quo)
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 remquo %v3float_2 %v3float_1 %in_quo
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z6remquoDv3_fS_PU3AS3Dv3_i(<3 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <3 x float> <float 1.500000e+00, float 1.500000e+00, float 1.500000e+00>, ptr addrspace(3) %in_quo)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
