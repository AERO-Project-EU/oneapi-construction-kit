; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_s_sub_sat_v3i32.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_sub_sat_v3i32 "s_sub_sat_v3i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 1
             %v3i32_t = OpTypeVector %i32_t 3
      %s_sub_sat_fn_t = OpTypeFunction %void_t
               %i32_3 = OpConstant %i32_t 3
               %i32_4 = OpConstant %i32_t 4
             %v3i32_3 = OpConstantComposite %v3i32_t %i32_3 %i32_3 %i32_3
             %v3i32_4 = OpConstantComposite %v3i32_t %i32_4 %i32_4 %i32_4

; All functions
     %s_sub_sat_v3i32 = OpFunction %void_t None %s_sub_sat_fn_t
; CHECK: define spir_kernel void @s_sub_sat_v3i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v3i32_t %1 s_sub_sat %v3i32_3 %v3i32_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x i32> @_Z7sub_satDv3_iS_(<3 x i32> <i32 3, i32 3, i32 3>, <3 x i32> <i32 4, i32 4, i32 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
