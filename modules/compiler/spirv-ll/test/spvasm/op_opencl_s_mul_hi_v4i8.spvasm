; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int8
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_mul_hi_v4i8 "s_mul_hi_v4i8"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
                %i8_t = OpTypeInt 8 1
              %v4i8_t = OpTypeVector %i8_t 4
       %s_mul_hi_fn_t = OpTypeFunction %void_t
                %i8_1 = OpConstant %i8_t 1
                %i8_2 = OpConstant %i8_t 2
              %v4i8_1 = OpConstantComposite %v4i8_t %i8_1 %i8_1 %i8_1 %i8_1
              %v4i8_2 = OpConstantComposite %v4i8_t %i8_2 %i8_2 %i8_2 %i8_2

; All functions
       %s_mul_hi_v4i8 = OpFunction %void_t None %s_mul_hi_fn_t
; CHECK: define spir_kernel void @s_mul_hi_v4i8()
                   %2 = OpLabel
                   %3 = OpExtInst %v4i8_t %1 s_mul_hi %v4i8_1 %v4i8_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x i8> @_Z6mul_hiDv4_cS_(<4 x i8> <i8 1, i8 1, i8 1, i8 1>, <4 x i8> <i8 2, i8 2, i8 2, i8 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
