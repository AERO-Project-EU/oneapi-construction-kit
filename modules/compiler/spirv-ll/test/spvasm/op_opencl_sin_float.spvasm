; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_sin_float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %sin_float "sin_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
            %sin_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.57

; All functions
           %sin_float = OpFunction %void_t None %sin_fn_t
; CHECK: define spir_kernel void @sin_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 sin %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z3sinf(float 0x3FF91EB860000000)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
