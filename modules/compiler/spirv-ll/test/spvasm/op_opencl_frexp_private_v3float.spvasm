; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_frexp_private_v3float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %frexp_v3float "frexp_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
             %v3int_t = OpTypeVector %int_t 3
           %v3float_t = OpTypeVector %float_t 3
         %v3int_ptr_t = OpTypePointer Function %v3int_t
          %frexp_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
       %frexp_v3float = OpFunction %void_t None %frexp_fn_t
; CHECK: define spir_kernel void @frexp_v3float()
                   %2 = OpLabel
                   %3 = OpVariable %v3int_ptr_t Function
; CHECK: [[ptr:%[a-zA-Z0-9]+]] = alloca <3 x i32>
                   %4 = OpExtInst %v3float_t %1 frexp %v3float_1 %3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z5frexpDv3_fPDv3_i(<3 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr [[ptr]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
