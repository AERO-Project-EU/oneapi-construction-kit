; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %hypot_float "hypot_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
          %hypot_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
             %float_2 = OpConstant %float_t 2

; All functions
         %hypot_float = OpFunction %void_t None %hypot_fn_t
; CHECK: define spir_kernel void @hypot_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 hypot %float_2 %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z5hypotff(float 2.000000e+00, float 1.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
