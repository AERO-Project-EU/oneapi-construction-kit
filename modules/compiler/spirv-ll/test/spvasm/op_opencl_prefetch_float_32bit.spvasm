; RUN: %spirv-ll -a OpenCL -b 32 %p/op_opencl_prefetch_float_32bit.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical32 OpenCL
                        OpEntryPoint Kernel %prefetch_float "prefetch_float"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
               %i32_t = OpTypeInt 32 0
         %float_ptr_t = OpTypePointer CrossWorkgroup %float_t
       %prefetch_fn_t = OpTypeFunction %void_t %float_ptr_t
               %i32_1 = OpConstant %i32_t 2

; All functions
      %prefetch_float = OpFunction %void_t None %prefetch_fn_t
              %in_ptr = OpFunctionParameter %float_ptr_t
; CHECK: define spir_kernel void @prefetch_float(ptr addrspace(1) %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %void_t %1 prefetch %in_ptr %i32_1
; CHECK: call spir_func void @_Z8prefetchPU3AS1Kfj(ptr addrspace(1) %in_ptr, i32 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
