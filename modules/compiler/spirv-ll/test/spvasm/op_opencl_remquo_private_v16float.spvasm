; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_remquo_private_v16float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %remquo_v16float "remquo_v16float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
            %v16int_t = OpTypeVector %int_t 16
          %v16float_t = OpTypeVector %float_t 16
        %v16int_ptr_t = OpTypePointer Function %v16int_t
         %remquo_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.5
             %float_2 = OpConstant %float_t 2.0
          %v16float_1 = OpConstantComposite %v16float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1
          %v16float_2 = OpConstantComposite %v16float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2

; All functions
     %remquo_v16float = OpFunction %void_t None %remquo_fn_t
; CHECK: define spir_kernel void @remquo_v16float()
                   %2 = OpLabel
                   %3 = OpVariable %v16int_ptr_t Function
; CHECK: [[quo:%[a-zA-Z0-9]+]] = alloca <16 x i32>
                   %4 = OpExtInst %v16float_t %1 remquo %v16float_2 %v16float_1 %3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x float> @_Z6remquoDv16_fS_PDv16_i(<16 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <16 x float> <float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00>, ptr [[quo]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
