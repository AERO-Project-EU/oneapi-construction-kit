; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s

                  OpCapability Kernel
                  OpCapability Addresses
             %1 = OpExtInstImport "OpenCL.std"
                  OpMemoryModel Physical64 OpenCL
                  OpEntryPoint Kernel %foo "metadata"
                  OpExecutionMode %foo LocalSizeHint 32 16 1
                  OpSource OpenCL_C 102000
        %void_t = OpTypeVoid
         %foo_t = OpTypeFunction %void_t
           %foo = OpFunction %void_t None %foo_t
; CHECK: !work_group_size_hint [[MD:![0-9]+]]
             %2 = OpLabel
                  OpReturn
                  OpFunctionEnd
; CHECK: [[MD]] = !{i32 32, i32 16, i32 1}
