; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                          OpCapability Kernel
                          OpCapability Addresses
                     %1 = OpExtInstImport "OpenCL.std"
                          OpMemoryModel Physical64 OpenCL
                          OpEntryPoint Kernel %fast_normalize_v2float "fast_normalize_v2float"
                          OpSource OpenCL_C 102000

; All types, variables and constants
                %void_t = OpTypeVoid
               %float_t = OpTypeFloat 32
             %v2float_t = OpTypeVector %float_t 2
   %fast_normalize_fn_t = OpTypeFunction %void_t
               %float_5 = OpConstant %float_t 5.0
             %v2float_5 = OpConstantComposite %v2float_t %float_5 %float_5

; All functions
%fast_normalize_v2float = OpFunction %void_t None %fast_normalize_fn_t
; CHECK: define spir_kernel void @fast_normalize_v2float()
                     %2 = OpLabel
                     %3 = OpExtInst %v2float_t %1 fast_normalize %v2float_5
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x float> @_Z14fast_normalizeDv2_f(<2 x float> <float 5.000000e+00, float 5.000000e+00>)
                          OpReturn
; CHECK: ret void
                          OpFunctionEnd
; CHECK: }
