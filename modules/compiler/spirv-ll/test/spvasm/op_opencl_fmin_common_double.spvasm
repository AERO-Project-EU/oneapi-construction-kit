; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fmin_common_double "fmin_common_double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
    %fmin_common_fn_t = OpTypeFunction %void_t
            %double_1 = OpConstant %double_t 1.9
            %double_2 = OpConstant %double_t 2.4

; All functions
  %fmin_common_double = OpFunction %void_t None %fmin_common_fn_t
; CHECK: define spir_kernel void @fmin_common_double()
                   %2 = OpLabel
                   %3 = OpExtInst %double_t %1 fmin_common %double_1 %double_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func double @_Z3mindd(double 1.900000e+00, double 2.400000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
