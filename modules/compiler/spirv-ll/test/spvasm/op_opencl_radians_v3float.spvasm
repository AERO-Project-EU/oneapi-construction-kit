; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_radians_v3float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %radians_v3float "radians_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
        %radians_fn_t = OpTypeFunction %void_t
            %float_90 = OpConstant %float_t 90.0
          %v3float_90 = OpConstantComposite %v3float_t %float_90 %float_90 %float_90

; All functions
     %radians_v3float = OpFunction %void_t None %radians_fn_t
; CHECK: define spir_kernel void @radians_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 radians %v3float_90
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z7radiansDv3_f(<3 x float> <float 9.000000e+01, float 9.000000e+01, float 9.000000e+01>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
