; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_abs_v8i32.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_abs_v8i32 "u_abs_v8i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
             %v8i32_t = OpTypeVector %i32_t 8
          %u_abs_fn_t = OpTypeFunction %void_t
               %i32_2 = OpConstant %i32_t 2
             %v8i32_2 = OpConstantComposite %v8i32_t %i32_2 %i32_2 %i32_2 %i32_2 %i32_2 %i32_2 %i32_2 %i32_2

; All functions
         %u_abs_v8i32 = OpFunction %void_t None %u_abs_fn_t
; CHECK: define spir_kernel void @u_abs_v8i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v8i32_t %1 u_abs %v8i32_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x i32> @_Z3absDv8_j(<8 x i32> <i32 2, i32 2, i32 2, i32 2, i32 2, i32 2, i32 2, i32 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
