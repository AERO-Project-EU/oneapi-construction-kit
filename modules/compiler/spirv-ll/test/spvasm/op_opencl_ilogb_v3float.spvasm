; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_ilogb_v3float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %ilogb_v3float "ilogb_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
             %v3int_t = OpTypeVector %int_t 3
           %v3float_t = OpTypeVector %float_t 3
          %ilogb_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
       %ilogb_v3float = OpFunction %void_t None %ilogb_fn_t
; CHECK: define spir_kernel void @ilogb_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3int_t %1 ilogb %v3float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x i32> @_Z5ilogbDv3_f(<3 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
