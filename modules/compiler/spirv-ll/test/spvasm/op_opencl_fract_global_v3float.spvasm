; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_fract_global_v3float.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fract_v3float "fract_v3float"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
       %v3float_ptr_t = OpTypePointer CrossWorkgroup %v3float_t
          %fract_fn_t = OpTypeFunction %void_t %v3float_ptr_t
             %float_1 = OpConstant %float_t 1.4
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
       %fract_v3float = OpFunction %void_t None %fract_fn_t
              %in_ptr = OpFunctionParameter %v3float_ptr_t
; CHECK-GE15: define spir_kernel void @fract_v3float(ptr addrspace(1) %in_ptr)
; CHECK-LT15: define spir_kernel void @fract_v3float(<3 x float> addrspace(1)* %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 fract %v3float_1 %in_ptr
; CHECK-GE15: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z5fractDv3_fPU3AS1S_(<3 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr addrspace(1) %in_ptr)
; CHECK-LT15: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z5fractDv3_fPU3AS1S_(<3 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, <3 x float> addrspace(1)* %in_ptr)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
