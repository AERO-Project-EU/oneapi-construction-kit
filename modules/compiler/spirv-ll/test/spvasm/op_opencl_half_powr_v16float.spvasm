; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_powr_v16float "half_powr_v16float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
          %v16float_t = OpTypeVector %float_t 16
      %half_powr_fn_t = OpTypeFunction %void_t
             %float_0 = OpConstant %float_t 0.5
             %float_2 = OpConstant %float_t 2.0
          %v16float_0 = OpConstantComposite %v16float_t %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0 %float_0
          %v16float_2 = OpConstantComposite %v16float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2

; All functions
  %half_powr_v16float = OpFunction %void_t None %half_powr_fn_t
; CHECK: define spir_kernel void @half_powr_v16float()
                   %2 = OpLabel
                   %3 = OpExtInst %v16float_t %1 half_powr %v16float_2 %v16float_0
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x float> @_Z9half_powrDv16_fS_(<16 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <16 x float> <float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01, float 5.000000e-01>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
