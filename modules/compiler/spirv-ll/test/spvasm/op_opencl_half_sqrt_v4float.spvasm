; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_half_sqrt_v4float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_sqrt_v4float "half_sqrt_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
      %half_sqrt_fn_t = OpTypeFunction %void_t
             %float_9 = OpConstant %float_t 9.0
           %v4float_9 = OpConstantComposite %v4float_t %float_9 %float_9 %float_9 %float_9

; All functions
   %half_sqrt_v4float = OpFunction %void_t None %half_sqrt_fn_t
; CHECK: define spir_kernel void @half_sqrt_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 half_sqrt %v4float_9
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z9half_sqrtDv4_f(<4 x float> <float 9.000000e+00, float 9.000000e+00, float 9.000000e+00, float 9.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
