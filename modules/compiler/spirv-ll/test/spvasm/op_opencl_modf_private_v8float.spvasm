; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_modf_private_v8float.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %modf_v8float "modf_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
       %v8float_ptr_t = OpTypePointer Function %v8float_t
           %modf_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1

; All functions
        %modf_v8float = OpFunction %void_t None %modf_fn_t
; CHECK: define spir_kernel void @modf_v8float()
                   %2 = OpLabel
                   %3 = OpVariable %v8float_ptr_t Function
; CHECK: [[iptr:%[a-zA-Z0-9]+]] = alloca <8 x float>
                   %4 = OpExtInst %v8float_t %1 modf %v8float_1 %3
; CHECK-GE15: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z4modfDv8_fPS_(<8 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr [[iptr]])
; CHECK-LT15: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z4modfDv8_fPS_(<8 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, <8 x float>* [[iptr]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
