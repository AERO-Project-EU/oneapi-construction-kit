; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fdim_v3float "fdim_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
           %fdim_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
             %float_2 = OpConstant %float_t 2
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1
           %v3float_2 = OpConstantComposite %v3float_t %float_2 %float_2 %float_2

; All functions
        %fdim_v3float = OpFunction %void_t None %fdim_fn_t
; CHECK: define spir_kernel void @fdim_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 fdim %v3float_2 %v3float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z4fdimDv3_fS_(<3 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <3 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
