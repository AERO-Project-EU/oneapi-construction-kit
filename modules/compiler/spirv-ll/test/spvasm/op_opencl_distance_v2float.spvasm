; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_distance_v2float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %distance_v2float "distance_v2float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v2float_t = OpTypeVector %float_t 2
       %distance_fn_t = OpTypeFunction %void_t
             %float_2 = OpConstant %float_t 2.0
             %float_4 = OpConstant %float_t 4.0
           %v2float_2 = OpConstantComposite %v2float_t %float_2 %float_2
           %v2float_4 = OpConstantComposite %v2float_t %float_4 %float_4

; All functions
    %distance_v2float = OpFunction %void_t None %distance_fn_t
; CHECK: define spir_kernel void @distance_v2float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 distance %v2float_2 %v2float_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z8distanceDv2_fS_(<2 x float> <float 2.000000e+00, float 2.000000e+00>, <2 x float> <float 4.000000e+00, float 4.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
