; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %normalize_v3float "normalize_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
      %normalize_fn_t = OpTypeFunction %void_t
             %float_5 = OpConstant %float_t 5.0
           %v3float_5 = OpConstantComposite %v3float_t %float_5 %float_5 %float_5

; All functions
   %normalize_v3float = OpFunction %void_t None %normalize_fn_t
; CHECK: define spir_kernel void @normalize_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 normalize %v3float_5
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z9normalizeDv3_f(<3 x float> <float 5.000000e+00, float 5.000000e+00, float 5.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
