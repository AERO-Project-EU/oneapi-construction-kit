; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fract_v3float "fract_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
       %v3float_ptr_t = OpTypePointer Function %v3float_t
          %fract_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
       %fract_v3float = OpFunction %void_t None %fract_fn_t
; CHECK: define spir_kernel void @fract_v3float()
                   %2 = OpLabel
                   %3 = OpVariable %v3float_ptr_t Function
; CHECK: [[ptr:%[a-zA-Z0-9]+]] = alloca <3 x float>
                   %4 = OpExtInst %v3float_t %1 fract %v3float_1 %3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z5fractDv3_fPS_(<3 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr [[ptr]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
