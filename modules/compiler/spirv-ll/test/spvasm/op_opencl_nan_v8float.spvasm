; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %nan_v8float "nan_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
              %uint_t = OpTypeInt 32 0
             %float_t = OpTypeFloat 32
            %v8uint_t = OpTypeVector %uint_t 8
           %v8float_t = OpTypeVector %float_t 8
            %nan_fn_t = OpTypeFunction %void_t
              %uint_3 = OpConstant %uint_t 3
            %v8uint_3 = OpConstantComposite %v8uint_t %uint_3 %uint_3 %uint_3 %uint_3 %uint_3 %uint_3 %uint_3 %uint_3

; All functions
         %nan_v8float = OpFunction %void_t None %nan_fn_t
; CHECK: define spir_kernel void @nan_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 nan %v8uint_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z3nanDv8_j(<8 x i32> <i32 3, i32 3, i32 3, i32 3, i32 3, i32 3, i32 3, i32 3>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
