; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_half_divide_v3float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_divide_v3float "half_divide_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
    %half_divide_fn_t = OpTypeFunction %void_t
             %float_7 = OpConstant %float_t 7.0
            %float_22 = OpConstant %float_t 22.0
           %v3float_7 = OpConstantComposite %v3float_t %float_7 %float_7 %float_7
          %v3float_22 = OpConstantComposite %v3float_t %float_22 %float_22 %float_22

; All functions
 %half_divide_v3float = OpFunction %void_t None %half_divide_fn_t
; CHECK: define spir_kernel void @half_divide_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 half_divide %v3float_22 %v3float_7
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z11half_divideDv3_fS_(<3 x float> <float 2.200000e+01, float 2.200000e+01, float 2.200000e+01>, <3 x float> <float 7.000000e+00, float 7.000000e+00, float 7.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
