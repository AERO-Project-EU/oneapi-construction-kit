; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %sqrt_v8float "sqrt_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
           %sqrt_fn_t = OpTypeFunction %void_t
             %float_9 = OpConstant %float_t 9.0
           %v8float_9 = OpConstantComposite %v8float_t %float_9 %float_9 %float_9 %float_9 %float_9 %float_9 %float_9 %float_9

; All functions
        %sqrt_v8float = OpFunction %void_t None %sqrt_fn_t
; CHECK: define spir_kernel void @sqrt_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 sqrt %v8float_9
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z4sqrtDv8_f(<8 x float> <float 9.000000e+00, float 9.000000e+00, float 9.000000e+00, float 9.000000e+00, float 9.000000e+00, float 9.000000e+00, float 9.000000e+00, float 9.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
