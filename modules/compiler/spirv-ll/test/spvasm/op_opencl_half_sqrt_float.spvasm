; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_sqrt_float "half_sqrt_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
      %half_sqrt_fn_t = OpTypeFunction %void_t
             %float_9 = OpConstant %float_t 9.0

; All functions
     %half_sqrt_float = OpFunction %void_t None %half_sqrt_fn_t
; CHECK: define spir_kernel void @half_sqrt_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 half_sqrt %float_9
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z9half_sqrtf(float 9.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
