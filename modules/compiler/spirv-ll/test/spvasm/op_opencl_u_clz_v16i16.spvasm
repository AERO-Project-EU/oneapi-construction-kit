; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int16
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_clz_v16i16 "u_clz_v16i16"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i16_t = OpTypeInt 16 0
            %v16i16_t = OpTypeVector %i16_t 16
          %u_clz_fn_t = OpTypeFunction %void_t
               %i16_1 = OpConstant %i16_t 1
            %v16i16_1 = OpConstantComposite %v16i16_t %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1 %i16_1

; All functions
        %u_clz_v16i16 = OpFunction %void_t None %u_clz_fn_t
; CHECK: define spir_kernel void @u_clz_v16i16()
                   %2 = OpLabel
                   %3 = OpExtInst %v16i16_t %1 clz %v16i16_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x i16> @_Z3clzDv16_t(<16 x i16> <i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1, i16 1>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
