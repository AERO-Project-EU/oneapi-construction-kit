; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_length_v4float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %length_v4float "length_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
         %length_fn_t = OpTypeFunction %void_t
             %float_3 = OpConstant %float_t 3.0
           %v4float_3 = OpConstantComposite %v4float_t %float_3 %float_3 %float_3 %float_3

; All functions
      %length_v4float = OpFunction %void_t None %length_fn_t
; CHECK: define spir_kernel void @length_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 length %v4float_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z6lengthDv4_f(<4 x float> <float 3.000000e+00, float 3.000000e+00, float 3.000000e+00, float 3.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
