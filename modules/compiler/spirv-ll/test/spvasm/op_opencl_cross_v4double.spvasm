; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %cross_v4double "cross_v4double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v4double_t = OpTypeVector %double_t 4
          %cross_fn_t = OpTypeFunction %void_t
            %double_1 = OpConstant %double_t 1.0
            %double_2 = OpConstant %double_t 2.0
          %v4double_1 = OpConstantComposite %v4double_t %double_1 %double_1 %double_1 %double_1
          %v4double_2 = OpConstantComposite %v4double_t %double_2 %double_2 %double_2 %double_2

; All functions
      %cross_v4double = OpFunction %void_t None %cross_fn_t
; CHECK: define spir_kernel void @cross_v4double()
                   %2 = OpLabel
                   %3 = OpExtInst %v4double_t %1 cross %v4double_1 %v4double_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x double> @_Z5crossDv4_dS_(<4 x double> <double 1.000000e+00, double 1.000000e+00, double 1.000000e+00, double 1.000000e+00>, <4 x double> <double 2.000000e+00, double 2.000000e+00, double 2.000000e+00, double 2.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
