; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %p/op_opencl_cross_v3double.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %cross_v3double "cross_v3double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v3double_t = OpTypeVector %double_t 3
          %cross_fn_t = OpTypeFunction %void_t
            %double_1 = OpConstant %double_t 1.0
            %double_2 = OpConstant %double_t 2.0
          %v3double_1 = OpConstantComposite %v3double_t %double_1 %double_1 %double_1
          %v3double_2 = OpConstantComposite %v3double_t %double_2 %double_2 %double_2

; All functions
      %cross_v3double = OpFunction %void_t None %cross_fn_t
; CHECK: define spir_kernel void @cross_v3double()
                   %2 = OpLabel
                   %3 = OpExtInst %v3double_t %1 cross %v3double_1 %v3double_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x double> @_Z5crossDv3_dS_(<3 x double> <double 1.000000e+00, double 1.000000e+00, double 1.000000e+00>, <3 x double> <double 2.000000e+00, double 2.000000e+00, double 2.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
