; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_upsample_v4i64 "u_upsample_v4i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
               %i64_t = OpTypeInt 64 0
             %v4i32_t = OpTypeVector %i32_t 4
             %v4i64_t = OpTypeVector %i64_t 4
     %u_upsample_fn_t = OpTypeFunction %void_t
               %i32_3 = OpConstant %i32_t 3
               %i32_4 = OpConstant %i32_t 4
             %v4i32_3 = OpConstantComposite %v4i32_t %i32_3 %i32_3 %i32_3 %i32_3
             %v4i32_4 = OpConstantComposite %v4i32_t %i32_4 %i32_4 %i32_4 %i32_4

; All functions
    %u_upsample_v4i64 = OpFunction %void_t None %u_upsample_fn_t
; CHECK: define spir_kernel void @u_upsample_v4i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v4i64_t %1 u_upsample %v4i32_3 %v4i32_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x i64> @_Z8upsampleDv4_jS_(<4 x i32> <i32 3, i32 3, i32 3, i32 3>, <4 x i32> <i32 4, i32 4, i32 4, i32 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
