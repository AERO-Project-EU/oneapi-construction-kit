; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %bitselect_v2float "bitselect_v2float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v2float_t = OpTypeVector %float_t 2
      %bitselect_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.0
             %float_2 = OpConstant %float_t 2.0
             %float_3 = OpConstant %float_t 3.0
           %v2float_1 = OpConstantComposite %v2float_t %float_1 %float_1
           %v2float_2 = OpConstantComposite %v2float_t %float_2 %float_2
           %v2float_3 = OpConstantComposite %v2float_t %float_3 %float_3

; All functions
   %bitselect_v2float = OpFunction %void_t None %bitselect_fn_t
; CHECK: define spir_kernel void @bitselect_v2float()
                   %2 = OpLabel
                   %3 = OpExtInst %v2float_t %1 bitselect %v2float_1 %v2float_2 %v2float_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x float> @_Z9bitselectDv2_fS_S_(<2 x float> <float 1.000000e+00, float 1.000000e+00>, <2 x float> <float 2.000000e+00, float 2.000000e+00>, <2 x float> <float 3.000000e+00, float 3.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
