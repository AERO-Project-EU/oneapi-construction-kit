; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_remquo_global_v2float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %remquo_v2float "remquo_v2float"
                        OpSource OpenCL_C 102000
                        OpName %in_quo "in_quo"

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
             %v2int_t = OpTypeVector %int_t 2
           %v2float_t = OpTypeVector %float_t 2
         %v2int_ptr_t = OpTypePointer CrossWorkgroup %v2int_t
         %remquo_fn_t = OpTypeFunction %void_t %v2int_ptr_t
             %float_1 = OpConstant %float_t 1.5
             %float_2 = OpConstant %float_t 2.0
           %v2float_1 = OpConstantComposite %v2float_t %float_1 %float_1
           %v2float_2 = OpConstantComposite %v2float_t %float_2 %float_2

; All functions
      %remquo_v2float = OpFunction %void_t None %remquo_fn_t
              %in_quo = OpFunctionParameter %v2int_ptr_t
; CHECK: define spir_kernel void @remquo_v2float(ptr addrspace(1) %in_quo)
                   %2 = OpLabel
                   %3 = OpExtInst %v2float_t %1 remquo %v2float_2 %v2float_1 %in_quo
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x float> @_Z6remquoDv2_fS_PU3AS1Dv2_i(<2 x float> <float 2.000000e+00, float 2.000000e+00>, <2 x float> <float 1.500000e+00, float 1.500000e+00>, ptr addrspace(1) %in_quo)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
