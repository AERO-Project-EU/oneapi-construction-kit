; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 32 %p/op_opencl_u_prefetch_v2i32_32bit.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical32 OpenCL
                        OpEntryPoint Kernel %u_prefetch_v2i32 "u_prefetch_v2i32"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
             %v2i32_t = OpTypeVector %i32_t 2
         %v2i32_ptr_t = OpTypePointer CrossWorkgroup %v2i32_t
     %u_prefetch_fn_t = OpTypeFunction %void_t %v2i32_ptr_t
               %i32_1 = OpConstant %i32_t 2

; All functions
    %u_prefetch_v2i32 = OpFunction %void_t None %u_prefetch_fn_t
              %in_ptr = OpFunctionParameter %v2i32_ptr_t
; CHECK-GE15: define spir_kernel void @u_prefetch_v2i32(ptr addrspace(1) %in_ptr)
; CHECK-LT15: define spir_kernel void @u_prefetch_v2i32(<2 x i32> addrspace(1)* %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %void_t %1 prefetch %in_ptr %i32_1
; CHECK-GE15: call spir_func void @_Z8prefetchPU3AS1KDv2_jj(ptr addrspace(1) %in_ptr, i32 2)
; CHECK-LT15: call spir_func void @_Z8prefetchPU3AS1KDv2_jj(<2 x i32> addrspace(1)* %in_ptr, i32 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
