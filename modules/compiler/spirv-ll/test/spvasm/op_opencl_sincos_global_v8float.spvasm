; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %sincos_v8float "sincos_v8float"
                        OpSource OpenCL_C 102000
                        OpName %in_cosval "in_cosval"

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
       %v8float_ptr_t = OpTypePointer CrossWorkgroup %v8float_t
         %sincos_fn_t = OpTypeFunction %void_t %v8float_ptr_t
             %float_1 = OpConstant %float_t 1.05
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1

; All functions
      %sincos_v8float = OpFunction %void_t None %sincos_fn_t
           %in_cosval = OpFunctionParameter %v8float_ptr_t
; CHECK: define spir_kernel void @sincos_v8float(ptr addrspace(1) %in_cosval)
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 sincos %v8float_1 %in_cosval
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z6sincosDv8_fPU3AS1S_(<8 x float> <float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000>, ptr addrspace(1) %in_cosval)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
