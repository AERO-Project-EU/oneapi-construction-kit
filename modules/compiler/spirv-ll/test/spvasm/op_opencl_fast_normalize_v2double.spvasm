; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %spv_file_s | %filecheck %s
                            OpCapability Kernel
                            OpCapability Addresses
                            OpCapability Float64
                       %1 = OpExtInstImport "OpenCL.std"
                            OpMemoryModel Physical64 OpenCL
                            OpEntryPoint Kernel %fast_normalize_v2double "fast_normalize_v2double"
                            OpSource OpenCL_C 102000

; All types, variables and constants
                  %void_t = OpTypeVoid
                %double_t = OpTypeFloat 64
              %v2double_t = OpTypeVector %double_t 2
     %fast_normalize_fn_t = OpTypeFunction %void_t
                %double_5 = OpConstant %double_t 5.0
              %v2double_5 = OpConstantComposite %v2double_t %double_5 %double_5

; All functions
 %fast_normalize_v2double = OpFunction %void_t None %fast_normalize_fn_t
; CHECK: define spir_kernel void @fast_normalize_v2double()
                       %2 = OpLabel
                       %3 = OpExtInst %v2double_t %1 fast_normalize %v2double_5
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x double> @_Z14fast_normalizeDv2_d(<2 x double> <double 5.000000e+00, double 5.000000e+00>)
                            OpReturn
; CHECK: ret void
                            OpFunctionEnd
; CHECK: }
