; RUN: %spirv-ll -a OpenCL -b 64 -c Addresses %spv_file_s | %filecheck %s
               OpCapability Addresses
               OpCapability Kernel
               OpCapability Vector16
               OpCapability Int64
          %1 = OpExtInstImport "OpenCL.std"
               OpMemoryModel Physical64 OpenCL
               OpEntryPoint Kernel %7 "foo"
               OpSource OpenCL_C 102000
               OpName %out "out"
               OpName %loc "loc"
               OpName %entry "entry"
       %uint = OpTypeInt 32 0
      %ulong = OpTypeInt 64 0
     %uint_2 = OpConstant %uint 2
    %ulong_0 = OpConstant %ulong 0
     %uint_3 = OpConstant %uint 3
     %uint_4 = OpConstant %uint 4
     %uint_8 = OpConstant %uint 8
    %uint_16 = OpConstant %uint 16
       %void = OpTypeVoid
%_ptr_CrossWorkgroup_uint = OpTypePointer CrossWorkgroup %uint
%_ptr_Workgroup_uint = OpTypePointer Workgroup %uint
          %6 = OpTypeFunction %void %_ptr_CrossWorkgroup_uint %_ptr_Workgroup_uint
     %v2uint = OpTypeVector %uint 2
     %v3uint = OpTypeVector %uint 3
     %v4uint = OpTypeVector %uint 4
     %v8uint = OpTypeVector %uint 8
    %v16uint = OpTypeVector %uint 16
         %13 = OpConstantComposite %v2uint %uint_2 %uint_2
         %19 = OpConstantComposite %v3uint %uint_3 %uint_3 %uint_3
         %23 = OpConstantComposite %v4uint %uint_4 %uint_4 %uint_4 %uint_4
         %27 = OpConstantComposite %v8uint %uint_8 %uint_8 %uint_8 %uint_8 %uint_8 %uint_8 %uint_8 %uint_8
         %31 = OpConstantComposite %v16uint %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16 %uint_16
          %7 = OpFunction %void None %6
        %out = OpFunctionParameter %_ptr_CrossWorkgroup_uint
        %loc = OpFunctionParameter %_ptr_Workgroup_uint
      %entry = OpLabel
         %16 = OpExtInst %void %1 vstoren %13 %ulong_0 %out
; CHECK: call spir_func void @_Z7vstore2Dv2_jmPU3AS1j(<2 x i32> <i32 2, i32 2>, i64 0, ptr addrspace(1) %out)
         %20 = OpExtInst %void %1 vstoren %19 %ulong_0 %out
; CHECK: call spir_func void @_Z7vstore3Dv3_jmPU3AS1j(<3 x i32> <i32 3, i32 3, i32 3>, i64 0, ptr addrspace(1) %out)
         %24 = OpExtInst %void %1 vstoren %23 %ulong_0 %out
; CHECK: call spir_func void @_Z7vstore4Dv4_jmPU3AS1j(<4 x i32> <i32 4, i32 4, i32 4, i32 4>, i64 0, ptr addrspace(1) %out)
         %28 = OpExtInst %void %1 vstoren %27 %ulong_0 %out
; CHECK: call spir_func void @_Z7vstore8Dv8_jmPU3AS1j(<8 x i32> <i32 8, i32 8, i32 8, i32 8, i32 8, i32 8, i32 8, i32 8>, i64 0, ptr addrspace(1) %out)
         %32 = OpExtInst %void %1 vstoren %31 %ulong_0 %out
; CHECK: call spir_func void @_Z8vstore16Dv16_jmPU3AS1j(<16 x i32> <i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16>, i64 0, ptr addrspace(1) %out)
         %33 = OpExtInst %void %1 vstoren %13 %ulong_0 %loc
; CHECK: call spir_func void @_Z7vstore2Dv2_jmPU3AS3j(<2 x i32> <i32 2, i32 2>, i64 0, ptr addrspace(3) %loc)
         %34 = OpExtInst %void %1 vstoren %19 %ulong_0 %loc
; CHECK: call spir_func void @_Z7vstore3Dv3_jmPU3AS3j(<3 x i32> <i32 3, i32 3, i32 3>, i64 0, ptr addrspace(3) %loc)
         %35 = OpExtInst %void %1 vstoren %23 %ulong_0 %loc
; CHECK: call spir_func void @_Z7vstore4Dv4_jmPU3AS3j(<4 x i32> <i32 4, i32 4, i32 4, i32 4>, i64 0, ptr addrspace(3) %loc)
         %36 = OpExtInst %void %1 vstoren %27 %ulong_0 %loc
; CHECK: call spir_func void @_Z7vstore8Dv8_jmPU3AS3j(<8 x i32> <i32 8, i32 8, i32 8, i32 8, i32 8, i32 8, i32 8, i32 8>, i64 0, ptr addrspace(3) %loc)
         %37 = OpExtInst %void %1 vstoren %31 %ulong_0 %loc
; CHECK: call spir_func void @_Z8vstore16Dv16_jmPU3AS3j(<16 x i32> <i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16, i32 16>, i64 0, ptr addrspace(3) %loc)
               OpReturn
               OpFunctionEnd
