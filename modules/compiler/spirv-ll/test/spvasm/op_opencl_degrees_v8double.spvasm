; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %p/op_opencl_degrees_v8double.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %degrees_v8double "degrees_v8double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v8double_t = OpTypeVector %double_t 8
        %degrees_fn_t = OpTypeFunction %void_t
            %double_1 = OpConstant %double_t 1.57
          %v8double_1 = OpConstantComposite %v8double_t %double_1 %double_1 %double_1 %double_1 %double_1 %double_1 %double_1 %double_1

; All functions
    %degrees_v8double = OpFunction %void_t None %degrees_fn_t
; CHECK: define spir_kernel void @degrees_v8double()
                   %2 = OpLabel
                   %3 = OpExtInst %v8double_t %1 degrees %v8double_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x double> @_Z7degreesDv8_d(<8 x double> <double 1.570000e+00, double 1.570000e+00, double 1.570000e+00, double 1.570000e+00, double 1.570000e+00, double 1.570000e+00, double 1.570000e+00, double 1.570000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
