; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %length_v3float "length_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
         %length_fn_t = OpTypeFunction %void_t
             %float_3 = OpConstant %float_t 3.0
           %v3float_3 = OpConstantComposite %v3float_t %float_3 %float_3 %float_3

; All functions
      %length_v3float = OpFunction %void_t None %length_fn_t
; CHECK: define spir_kernel void @length_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 length %v3float_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z6lengthDv3_f(<3 x float> <float 3.000000e+00, float 3.000000e+00, float 3.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
