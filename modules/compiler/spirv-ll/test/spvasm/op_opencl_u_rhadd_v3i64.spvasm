; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_rhadd_v3i64 "u_rhadd_v3i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
             %v3i64_t = OpTypeVector %i64_t 3
        %u_rhadd_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1
               %i64_2 = OpConstant %i64_t 2
             %v3i64_1 = OpConstantComposite %v3i64_t %i64_1 %i64_1 %i64_1
             %v3i64_2 = OpConstantComposite %v3i64_t %i64_2 %i64_2 %i64_2

; All functions
       %u_rhadd_v3i64 = OpFunction %void_t None %u_rhadd_fn_t
; CHECK: define spir_kernel void @u_rhadd_v3i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v3i64_t %1 u_rhadd %v3i64_1 %v3i64_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x i64> @_Z5rhaddDv3_mS_(<3 x i64> <i64 1, i64 1, i64 1>, <3 x i64> <i64 2, i64 2, i64 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
