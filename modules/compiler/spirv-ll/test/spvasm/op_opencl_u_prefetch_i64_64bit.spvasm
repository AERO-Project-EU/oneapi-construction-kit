; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_prefetch_i64_64bit.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_prefetch_i64 "u_prefetch_i64"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
           %i64_ptr_t = OpTypePointer CrossWorkgroup %i64_t
     %u_prefetch_fn_t = OpTypeFunction %void_t %i64_ptr_t
               %i64_1 = OpConstant %i64_t 2

; All functions
      %u_prefetch_i64 = OpFunction %void_t None %u_prefetch_fn_t
              %in_ptr = OpFunctionParameter %i64_ptr_t
; CHECK: define spir_kernel void @u_prefetch_i64(ptr addrspace(1) %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %void_t %1 prefetch %in_ptr %i64_1
; CHECK: call spir_func void @_Z8prefetchPU3AS1Kmm(ptr addrspace(1) %in_ptr, i64 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
