; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_minmag_v4float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %minmag_v4float "minmag_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
         %minmag_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.2
             %float_2 = OpConstant %float_t -2.1
           %v4float_1 = OpConstantComposite %v4float_t %float_1 %float_1 %float_1 %float_1
           %v4float_2 = OpConstantComposite %v4float_t %float_2 %float_2 %float_2 %float_2

; All functions
      %minmag_v4float = OpFunction %void_t None %minmag_fn_t
; CHECK: define spir_kernel void @minmag_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 minmag %v4float_2 %v4float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z6minmagDv4_fS_(<4 x float> <float 0xC000CCCCC0000000, float 0xC000CCCCC0000000, float 0xC000CCCCC0000000, float 0xC000CCCCC0000000>, <4 x float> <float 0x3FF3333340000000, float 0x3FF3333340000000, float 0x3FF3333340000000, float 0x3FF3333340000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
