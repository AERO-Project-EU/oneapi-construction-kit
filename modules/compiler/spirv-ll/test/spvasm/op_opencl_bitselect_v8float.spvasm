; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %bitselect_v8float "bitselect_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
      %bitselect_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.0
             %float_2 = OpConstant %float_t 2.0
             %float_3 = OpConstant %float_t 3.0
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1
           %v8float_2 = OpConstantComposite %v8float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2
           %v8float_3 = OpConstantComposite %v8float_t %float_3 %float_3 %float_3 %float_3 %float_3 %float_3 %float_3 %float_3

; All functions
   %bitselect_v8float = OpFunction %void_t None %bitselect_fn_t
; CHECK: define spir_kernel void @bitselect_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 bitselect %v8float_1 %v8float_2 %v8float_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z9bitselectDv8_fS_S_(<8 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>, <8 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <8 x float> <float 3.000000e+00, float 3.000000e+00, float 3.000000e+00, float 3.000000e+00, float 3.000000e+00, float 3.000000e+00, float 3.000000e+00, float 3.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
