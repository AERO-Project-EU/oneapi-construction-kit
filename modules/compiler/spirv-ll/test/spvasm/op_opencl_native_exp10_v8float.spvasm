; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %native_exp10_v8float "native_exp10_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
   %native_exp10_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.0
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1

; All functions
%native_exp10_v8float = OpFunction %void_t None %native_exp10_fn_t
; CHECK: define spir_kernel void @native_exp10_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 native_exp10 %v8float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z12native_exp10Dv8_f(<8 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
