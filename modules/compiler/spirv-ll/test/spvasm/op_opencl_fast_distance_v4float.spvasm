; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                          OpCapability Kernel
                          OpCapability Addresses
                     %1 = OpExtInstImport "OpenCL.std"
                          OpMemoryModel Physical64 OpenCL
                          OpEntryPoint Kernel %fast_distance_v4float "fast_distance_v4float"
                          OpSource OpenCL_C 102000

; All types, variables and constants
                %void_t = OpTypeVoid
               %float_t = OpTypeFloat 32
             %v4float_t = OpTypeVector %float_t 4
    %fast_distance_fn_t = OpTypeFunction %void_t
               %float_2 = OpConstant %float_t 2.0
               %float_4 = OpConstant %float_t 4.0
             %v4float_2 = OpConstantComposite %v4float_t %float_2 %float_2 %float_2 %float_2
             %v4float_4 = OpConstantComposite %v4float_t %float_4 %float_4 %float_4 %float_4

; All functions
 %fast_distance_v4float = OpFunction %void_t None %fast_distance_fn_t
; CHECK: define spir_kernel void @fast_distance_v4float()
                     %2 = OpLabel
                     %3 = OpExtInst %float_t %1 fast_distance %v4float_2 %v4float_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z13fast_distanceDv4_fS_(<4 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <4 x float> <float 4.000000e+00, float 4.000000e+00, float 4.000000e+00, float 4.000000e+00>)
                          OpReturn
; CHECK: ret void
                          OpFunctionEnd
; CHECK: }
