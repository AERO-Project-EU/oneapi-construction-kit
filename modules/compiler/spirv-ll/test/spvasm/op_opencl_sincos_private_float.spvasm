; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %sincos_float "sincos_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
         %float_ptr_t = OpTypePointer Function %float_t
         %sincos_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.05

; All functions
        %sincos_float = OpFunction %void_t None %sincos_fn_t
; CHECK: define spir_kernel void @sincos_float()
                   %2 = OpLabel
                   %3 = OpVariable %float_ptr_t Function
; CHECK: [[cosval:%[a-zA-Z0-9]+]] = alloca float
                   %4 = OpExtInst %float_t %1 sincos %float_1 %3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z6sincosfPf(float 0x3FF0CCCCC0000000, ptr [[cosval]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
