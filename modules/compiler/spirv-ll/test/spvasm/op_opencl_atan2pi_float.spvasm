; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"                        
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %atan2pi_float "atan2pi_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
        %atan2pi_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
             %float_2 = OpConstant %float_t 2

; All functions
       %atan2pi_float = OpFunction %void_t None %atan2pi_fn_t
; CHECK: define spir_kernel void @atan2pi_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 atan2pi %float_1 %float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z7atan2piff(float 1.000000e+00, float 2.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
