; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_exp_v2float "half_exp_v2float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v2float_t = OpTypeVector %float_t 2
       %half_exp_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.0
           %v2float_1 = OpConstantComposite %v2float_t %float_1 %float_1

; All functions
    %half_exp_v2float = OpFunction %void_t None %half_exp_fn_t
; CHECK: define spir_kernel void @half_exp_v2float()
                   %2 = OpLabel
                   %3 = OpExtInst %v2float_t %1 half_exp %v2float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x float> @_Z8half_expDv2_f(<2 x float> <float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
