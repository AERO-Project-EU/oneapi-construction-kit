; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_ilogb_v4float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %ilogb_v4float "ilogb_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
             %v4int_t = OpTypeVector %int_t 4
           %v4float_t = OpTypeVector %float_t 4
          %ilogb_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
           %v4float_1 = OpConstantComposite %v4float_t %float_1 %float_1 %float_1 %float_1

; All functions
       %ilogb_v4float = OpFunction %void_t None %ilogb_fn_t
; CHECK: define spir_kernel void @ilogb_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %v4int_t %1 ilogb %v4float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x i32> @_Z5ilogbDv4_f(<4 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
