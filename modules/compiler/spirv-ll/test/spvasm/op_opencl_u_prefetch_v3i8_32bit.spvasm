; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 32 %p/op_opencl_u_prefetch_v3i8_32bit.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int8
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical32 OpenCL
                        OpEntryPoint Kernel %u_prefetch_v3i8 "u_prefetch_v3i8"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
                %i8_t = OpTypeInt 8 0
               %i32_t = OpTypeInt 32 0
              %v3i8_t = OpTypeVector %i8_t 3
          %v3i8_ptr_t = OpTypePointer CrossWorkgroup %v3i8_t
     %u_prefetch_fn_t = OpTypeFunction %void_t %v3i8_ptr_t
               %i32_1 = OpConstant %i32_t 2

; All functions
     %u_prefetch_v3i8 = OpFunction %void_t None %u_prefetch_fn_t
              %in_ptr = OpFunctionParameter %v3i8_ptr_t
; CHECK-GE15: define spir_kernel void @u_prefetch_v3i8(ptr addrspace(1) %in_ptr)
; CHECK-LT15: define spir_kernel void @u_prefetch_v3i8(<3 x i8> addrspace(1)* %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %void_t %1 prefetch %in_ptr %i32_1
; CHECK-GE15: call spir_func void @_Z8prefetchPU3AS1KDv3_hj(ptr addrspace(1) %in_ptr, i32 2)
; CHECK-LT15: call spir_func void @_Z8prefetchPU3AS1KDv3_hj(<3 x i8> addrspace(1)* %in_ptr, i32 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
