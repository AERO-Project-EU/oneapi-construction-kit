; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_sincos_private_v2float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %sincos_v2float "sincos_v2float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v2float_t = OpTypeVector %float_t 2
       %v2float_ptr_t = OpTypePointer Function %v2float_t
         %sincos_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.05
           %v2float_1 = OpConstantComposite %v2float_t %float_1 %float_1

; All functions
      %sincos_v2float = OpFunction %void_t None %sincos_fn_t
; CHECK: define spir_kernel void @sincos_v2float()
                   %2 = OpLabel
                   %3 = OpVariable %v2float_ptr_t Function
; CHECK: [[cosval:%[a-zA-Z0-9]+]] = alloca <2 x float>
                   %4 = OpExtInst %v2float_t %1 sincos %v2float_1 %3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x float> @_Z6sincosDv2_fPS_(<2 x float> <float 0x3FF0CCCCC0000000, float 0x3FF0CCCCC0000000>, ptr [[cosval]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
