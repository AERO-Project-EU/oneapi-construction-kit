; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int8
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_mul_hi_v3i8 "u_mul_hi_v3i8"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
                %i8_t = OpTypeInt 8 0
              %v3i8_t = OpTypeVector %i8_t 3
       %u_mul_hi_fn_t = OpTypeFunction %void_t
                %i8_2 = OpConstant %i8_t 2
                %i8_3 = OpConstant %i8_t 3
              %v3i8_2 = OpConstantComposite %v3i8_t %i8_2 %i8_2 %i8_2
              %v3i8_3 = OpConstantComposite %v3i8_t %i8_3 %i8_3 %i8_3

; All functions
       %u_mul_hi_v3i8 = OpFunction %void_t None %u_mul_hi_fn_t
; CHECK: define spir_kernel void @u_mul_hi_v3i8()
                   %2 = OpLabel
                   %3 = OpExtInst %v3i8_t %1 u_mul_hi %v3i8_2 %v3i8_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x i8> @_Z6mul_hiDv3_hS_(<3 x i8> <i8 2, i8 2, i8 2>, <3 x i8> <i8 3, i8 3, i8 3>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
