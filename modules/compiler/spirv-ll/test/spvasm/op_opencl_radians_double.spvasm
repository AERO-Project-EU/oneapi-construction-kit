; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %p/op_opencl_radians_double.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %radians_double "radians_double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
        %radians_fn_t = OpTypeFunction %void_t
           %double_90 = OpConstant %double_t 90.0

; All functions
      %radians_double = OpFunction %void_t None %radians_fn_t
; CHECK: define spir_kernel void @radians_double()
                   %2 = OpLabel
                   %3 = OpExtInst %double_t %1 radians %double_90
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func double @_Z7radiansd(double 9.000000e+01)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
