; RUN: %spirv-ll -a OpenCL -b 32 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical32 OpenCL
                        OpEntryPoint Kernel %u_prefetch_i16 "u_prefetch_i16"
                        OpSource OpenCL_C 102000
                        OpName %in_ptr "in_ptr"

; All types, variables and constants
              %void_t = OpTypeVoid
               %i16_t = OpTypeInt 16 0
               %i32_t = OpTypeInt 32 0
           %i16_ptr_t = OpTypePointer CrossWorkgroup %i16_t
     %u_prefetch_fn_t = OpTypeFunction %void_t %i16_ptr_t
               %i32_1 = OpConstant %i32_t 2

; All functions
      %u_prefetch_i16 = OpFunction %void_t None %u_prefetch_fn_t
              %in_ptr = OpFunctionParameter %i16_ptr_t
; CHECK: define spir_kernel void @u_prefetch_i16(ptr addrspace(1) %in_ptr)
                   %2 = OpLabel
                   %3 = OpExtInst %void_t %1 prefetch %in_ptr %i32_1
; CHECK: call spir_func void @_Z8prefetchPU3AS1Ktj(ptr addrspace(1) %in_ptr, i32 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
