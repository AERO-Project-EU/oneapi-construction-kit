; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_half_log_float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_log_float "half_log_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
       %half_log_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.4

; All functions
      %half_log_float = OpFunction %void_t None %half_log_fn_t
; CHECK: define spir_kernel void @half_log_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 half_log %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z8half_logf(float 0x3FF6666660000000)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
