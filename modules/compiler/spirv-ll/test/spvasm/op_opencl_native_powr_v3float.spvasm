; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_native_powr_v3float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %native_powr_v3float "native_powr_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
    %native_powr_fn_t = OpTypeFunction %void_t
             %float_0 = OpConstant %float_t 0.5
             %float_2 = OpConstant %float_t 2.0
           %v3float_0 = OpConstantComposite %v3float_t %float_0 %float_0 %float_0
           %v3float_2 = OpConstantComposite %v3float_t %float_2 %float_2 %float_2

; All functions
 %native_powr_v3float = OpFunction %void_t None %native_powr_fn_t
; CHECK: define spir_kernel void @native_powr_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 native_powr %v3float_2 %v3float_0
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z11native_powrDv3_fS_(<3 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <3 x float> <float 5.000000e-01, float 5.000000e-01, float 5.000000e-01>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
