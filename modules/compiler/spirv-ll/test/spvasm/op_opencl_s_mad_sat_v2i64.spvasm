; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_s_mad_sat_v2i64.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_mad_sat_v2i64 "s_mad_sat_v2i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 1
             %v2i64_t = OpTypeVector %i64_t 2
      %s_mad_sat_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1
               %i64_2 = OpConstant %i64_t 2
               %i64_3 = OpConstant %i64_t 3
             %v2i64_1 = OpConstantComposite %v2i64_t %i64_1 %i64_1
             %v2i64_2 = OpConstantComposite %v2i64_t %i64_2 %i64_2
             %v2i64_3 = OpConstantComposite %v2i64_t %i64_3 %i64_3

; All functions
     %s_mad_sat_v2i64 = OpFunction %void_t None %s_mad_sat_fn_t
; CHECK: define spir_kernel void @s_mad_sat_v2i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v2i64_t %1 s_mad_sat %v2i64_1 %v2i64_2 %v2i64_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x i64> @_Z7mad_satDv2_lS_S_(<2 x i64> <i64 1, i64 1>, <2 x i64> <i64 2, i64 2>, <2 x i64> <i64 3, i64 3>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
