; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_fclamp_float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fclamp_float "fclamp_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
         %fclamp_fn_t = OpTypeFunction %void_t
             %float_0 = OpConstant %float_t 0.8
             %float_1 = OpConstant %float_t 1.0
             %float_2 = OpConstant %float_t 2.0

; All functions
        %fclamp_float = OpFunction %void_t None %fclamp_fn_t
; CHECK: define spir_kernel void @fclamp_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 fclamp %float_0 %float_1 %float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z5clampfff(float 0x3FE99999A0000000, float 1.000000e+00, float 2.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
