; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_radians_float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %radians_float "radians_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
        %radians_fn_t = OpTypeFunction %void_t
            %float_90 = OpConstant %float_t 90.0

; All functions
       %radians_float = OpFunction %void_t None %radians_fn_t
; CHECK: define spir_kernel void @radians_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 radians %float_90
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z7radiansf(float 9.000000e+01)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
