; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_remquo_private_v8float.spv -o %t
; RUN: %filecheck < %t %t.pp
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %remquo_v8float "remquo_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
             %v8int_t = OpTypeVector %int_t 8
           %v8float_t = OpTypeVector %float_t 8
         %v8int_ptr_t = OpTypePointer Function %v8int_t
         %remquo_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.5
             %float_2 = OpConstant %float_t 2.0
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1
           %v8float_2 = OpConstantComposite %v8float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2

; All functions
      %remquo_v8float = OpFunction %void_t None %remquo_fn_t
; CHECK: define spir_kernel void @remquo_v8float()
                   %2 = OpLabel
                   %3 = OpVariable %v8int_ptr_t Function
; CHECK: [[quo:%[a-zA-Z0-9]+]] = alloca <8 x i32>
                   %4 = OpExtInst %v8float_t %1 remquo %v8float_2 %v8float_1 %3
; CHECK-GE15: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z6remquoDv8_fS_PDv8_i(<8 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <8 x float> <float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00>, ptr [[quo]])
; CHECK-LT15: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z6remquoDv8_fS_PDv8_i(<8 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <8 x float> <float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00, float 1.500000e+00>, <8 x i32>* [[quo]])
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
