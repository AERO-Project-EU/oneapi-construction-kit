; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_clz_v2i32.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_clz_v2i32 "u_clz_v2i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
             %v2i32_t = OpTypeVector %i32_t 2
          %u_clz_fn_t = OpTypeFunction %void_t
               %i32_1 = OpConstant %i32_t 1
             %v2i32_1 = OpConstantComposite %v2i32_t %i32_1 %i32_1

; All functions
         %u_clz_v2i32 = OpFunction %void_t None %u_clz_fn_t
; CHECK: define spir_kernel void @u_clz_v2i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v2i32_t %1 clz %v2i32_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x i32> @_Z3clzDv2_j(<2 x i32> <i32 1, i32 1>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
