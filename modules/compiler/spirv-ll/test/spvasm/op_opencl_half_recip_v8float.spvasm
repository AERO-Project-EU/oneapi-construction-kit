; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_half_recip_v8float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %half_recip_v8float "half_recip_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
     %half_recip_fn_t = OpTypeFunction %void_t
             %float_2 = OpConstant %float_t 2.0
           %v8float_2 = OpConstantComposite %v8float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2

; All functions
  %half_recip_v8float = OpFunction %void_t None %half_recip_fn_t
; CHECK: define spir_kernel void @half_recip_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 half_recip %v8float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z10half_recipDv8_f(<8 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
