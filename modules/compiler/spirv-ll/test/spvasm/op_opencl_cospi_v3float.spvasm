; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_cospi_v3float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"                        
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %cospi_v3float "cospi_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v3float_t = OpTypeVector %float_t 3
          %cospi_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
           %v3float_1 = OpConstantComposite %v3float_t %float_1 %float_1 %float_1

; All functions
       %cospi_v3float = OpFunction %void_t None %cospi_fn_t
; CHECK: define spir_kernel void @cospi_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 cospi %v3float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z5cospiDv3_f(<3 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
