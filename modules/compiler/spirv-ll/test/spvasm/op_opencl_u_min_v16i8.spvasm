; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_min_v16i8.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int8
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_min_v16i8 "u_min_v16i8"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
                %i8_t = OpTypeInt 8 0
             %v16i8_t = OpTypeVector %i8_t 16
          %u_min_fn_t = OpTypeFunction %void_t
                %i8_1 = OpConstant %i8_t 1
                %i8_2 = OpConstant %i8_t 2
             %v16i8_1 = OpConstantComposite %v16i8_t %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1 %i8_1
             %v16i8_2 = OpConstantComposite %v16i8_t %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2 %i8_2

; All functions
         %u_min_v16i8 = OpFunction %void_t None %u_min_fn_t
; CHECK: define spir_kernel void @u_min_v16i8()
                   %2 = OpLabel
                   %3 = OpExtInst %v16i8_t %1 u_min %v16i8_1 %v16i8_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x i8> @_Z3minDv16_hS_(<16 x i8> <i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1, i8 1>, <16 x i8> <i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2, i8 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
