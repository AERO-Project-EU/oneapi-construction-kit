; RUN: %pp-llvm-ver -o %t.pp < %s --llvm-ver %LLVMVER
; RUN: %spirv-ll -a Vulkan -c Float64 -c Int64 %p/op_vec_extract_v3float.spv -o %t
; RUN: %filecheck < %t %t.pp
            OpCapability Shader
            OpCapability Float64
            OpCapability Int64
       %1 = OpExtInstImport "GLSL.std.450"
            OpMemoryModel Logical GLSL450
            OpEntryPoint GLCompute %main "main"
            OpExecutionMode %main LocalSize 1 1 1
            OpSource GLSL 450
            OpName %main "main"
            OpName %a "a"
               OpName %res "res"
       %void = OpTypeVoid
          %3 = OpTypeFunction %void
        %float = OpTypeFloat 32
     %idx_ty = OpTypeInt 32 0
         %v3 = OpTypeVector %float 3
     %ptr_v3 = OpTypePointer Function %v3
          %8 = OpConstant %float 42.42
          %9 = OpConstant %float 42.42
         %10 = OpConstant %float 42.42
         %11 = OpConstant %idx_ty 1
         %12 = OpConstantComposite %v3 %8 %9 %10
    %ptr_float = OpTypePointer Function %float
       %main = OpFunction %void None %3
         %16 = OpLabel
          %a = OpVariable %ptr_v3 Function
        %res = OpVariable %ptr_float Function
               OpStore %a %12
         %19 = OpLoad %v3 %a
         %20 = OpVectorExtractDynamic %float %19 %11
               OpStore %res %20
               OpReturn
               OpFunctionEnd
; CHECK: ; ModuleID = '{{.*}}'
; CHECK: define spir_kernel void @main()
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = alloca <3 x float>
