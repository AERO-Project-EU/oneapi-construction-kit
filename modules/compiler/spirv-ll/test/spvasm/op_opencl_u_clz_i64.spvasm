; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_clz_i64.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_clz_i64 "u_clz_i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
          %u_clz_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1

; All functions
           %u_clz_i64 = OpFunction %void_t None %u_clz_fn_t
; CHECK: define spir_kernel void @u_clz_i64()
                   %2 = OpLabel
                   %3 = OpExtInst %i64_t %1 clz %i64_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func i64 @_Z3clzm(i64 1)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
