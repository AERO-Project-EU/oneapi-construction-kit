; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %p/op_opencl_bitselect_v8double.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %bitselect_v8double "bitselect_v8double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v8double_t = OpTypeVector %double_t 8
      %bitselect_fn_t = OpTypeFunction %void_t
            %double_1 = OpConstant %double_t 1.0
            %double_2 = OpConstant %double_t 2.0
            %double_3 = OpConstant %double_t 3.0
          %v8double_1 = OpConstantComposite %v8double_t %double_1 %double_1 %double_1 %double_1 %double_1 %double_1 %double_1 %double_1
          %v8double_2 = OpConstantComposite %v8double_t %double_2 %double_2 %double_2 %double_2 %double_2 %double_2 %double_2 %double_2
          %v8double_3 = OpConstantComposite %v8double_t %double_3 %double_3 %double_3 %double_3 %double_3 %double_3 %double_3 %double_3

; All functions
  %bitselect_v8double = OpFunction %void_t None %bitselect_fn_t
; CHECK: define spir_kernel void @bitselect_v8double()
                   %2 = OpLabel
                   %3 = OpExtInst %v8double_t %1 bitselect %v8double_1 %v8double_2 %v8double_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x double> @_Z9bitselectDv8_dS_S_(<8 x double> <double 1.000000e+00, double 1.000000e+00, double 1.000000e+00, double 1.000000e+00, double 1.000000e+00, double 1.000000e+00, double 1.000000e+00, double 1.000000e+00>, <8 x double> <double 2.000000e+00, double 2.000000e+00, double 2.000000e+00, double 2.000000e+00, double 2.000000e+00, double 2.000000e+00, double 2.000000e+00, double 2.000000e+00>, <8 x double> <double 3.000000e+00, double 3.000000e+00, double 3.000000e+00, double 3.000000e+00, double 3.000000e+00, double 3.000000e+00, double 3.000000e+00, double 3.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
