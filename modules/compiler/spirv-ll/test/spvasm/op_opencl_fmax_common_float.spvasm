; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fmax_common_float "fmax_common_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
    %fmax_common_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.9
             %float_2 = OpConstant %float_t 2.4

; All functions
   %fmax_common_float = OpFunction %void_t None %fmax_common_fn_t
; CHECK: define spir_kernel void @fmax_common_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 fmax_common %float_1 %float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z3maxff(float 0x3FFE666660000000, float 0x4003333340000000)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
