; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %tanpi_float "tanpi_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
          %tanpi_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.0

; All functions
         %tanpi_float = OpFunction %void_t None %tanpi_fn_t
; CHECK: define spir_kernel void @tanpi_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 tanpi %float_1
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z5tanpif(float 1.000000e+00)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
