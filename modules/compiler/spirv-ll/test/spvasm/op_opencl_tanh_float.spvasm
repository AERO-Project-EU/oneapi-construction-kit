; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_tanh_float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %tanh_float "tanh_float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %tanh_fn_t = OpTypeFunction %void_t
             %float_0 = OpConstant %float_t 0.79

; All functions
          %tanh_float = OpFunction %void_t None %tanh_fn_t
; CHECK: define spir_kernel void @tanh_float()
                   %2 = OpLabel
                   %3 = OpExtInst %float_t %1 tanh %float_0
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func float @_Z4tanhf(float 0x3FE947AE20000000)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
