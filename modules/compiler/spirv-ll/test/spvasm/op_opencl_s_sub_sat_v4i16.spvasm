; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %s_sub_sat_v4i16 "s_sub_sat_v4i16"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i16_t = OpTypeInt 16 1
             %v4i16_t = OpTypeVector %i16_t 4
      %s_sub_sat_fn_t = OpTypeFunction %void_t
               %i16_3 = OpConstant %i16_t 3
               %i16_4 = OpConstant %i16_t 4
             %v4i16_3 = OpConstantComposite %v4i16_t %i16_3 %i16_3 %i16_3 %i16_3
             %v4i16_4 = OpConstantComposite %v4i16_t %i16_4 %i16_4 %i16_4 %i16_4

; All functions
     %s_sub_sat_v4i16 = OpFunction %void_t None %s_sub_sat_fn_t
; CHECK: define spir_kernel void @s_sub_sat_v4i16()
                   %2 = OpLabel
                   %3 = OpExtInst %v4i16_t %1 s_sub_sat %v4i16_3 %v4i16_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x i16> @_Z7sub_satDv4_sS_(<4 x i16> <i16 3, i16 3, i16 3, i16 3>, <4 x i16> <i16 4, i16 4, i16 4, i16 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
