; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %radians_v16float "radians_v16float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
          %v16float_t = OpTypeVector %float_t 16
        %radians_fn_t = OpTypeFunction %void_t
            %float_90 = OpConstant %float_t 90.0
         %v16float_90 = OpConstantComposite %v16float_t %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90 %float_90

; All functions
    %radians_v16float = OpFunction %void_t None %radians_fn_t
; CHECK: define spir_kernel void @radians_v16float()
                   %2 = OpLabel
                   %3 = OpExtInst %v16float_t %1 radians %v16float_90
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x float> @_Z7radiansDv16_f(<16 x float> <float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01, float 9.000000e+01>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
