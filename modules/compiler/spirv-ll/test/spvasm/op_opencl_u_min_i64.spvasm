; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_min_i64 "u_min_i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
          %u_min_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1
               %i64_2 = OpConstant %i64_t 2

; All functions
           %u_min_i64 = OpFunction %void_t None %u_min_fn_t
; CHECK: define spir_kernel void @u_min_i64()
                   %2 = OpLabel
                   %3 = OpExtInst %i64_t %1 u_min %i64_1 %i64_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func i64 @_Z3minmm(i64 1, i64 2)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
