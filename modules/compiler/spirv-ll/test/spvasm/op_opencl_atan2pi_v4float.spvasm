; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_atan2pi_v4float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"                        
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %atan2pi_v4float "atan2pi_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
        %atan2pi_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1
             %float_2 = OpConstant %float_t 2
           %v4float_1 = OpConstantComposite %v4float_t %float_1 %float_1 %float_1 %float_1
           %v4float_2 = OpConstantComposite %v4float_t %float_2 %float_2 %float_2 %float_2

; All functions
     %atan2pi_v4float = OpFunction %void_t None %atan2pi_fn_t
; CHECK: define spir_kernel void @atan2pi_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 atan2pi %v4float_1 %v4float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z7atan2piDv4_fS_(<4 x float> <float 1.000000e+00, float 1.000000e+00, float 1.000000e+00, float 1.000000e+00>, <4 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
