; RUN: %spirv-ll -a OpenCL -b 64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fmax_common_v4float "fmax_common_v4float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
    %fmax_common_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.9
             %float_2 = OpConstant %float_t 2.4
           %v4float_1 = OpConstantComposite %v4float_t %float_1 %float_1 %float_1 %float_1
           %v4float_2 = OpConstantComposite %v4float_t %float_2 %float_2 %float_2 %float_2

; All functions
 %fmax_common_v4float = OpFunction %void_t None %fmax_common_fn_t
; CHECK: define spir_kernel void @fmax_common_v4float()
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 fmax_common %v4float_1 %v4float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z3maxDv4_fS_(<4 x float> <float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000>, <4 x float> <float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
