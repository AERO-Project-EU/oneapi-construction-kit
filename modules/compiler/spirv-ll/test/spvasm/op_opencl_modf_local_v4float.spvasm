; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_modf_local_v4float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %modf_v4float "modf_v4float"
                        OpSource OpenCL_C 102000
                        OpName %in_iptr "in_iptr"

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v4float_t = OpTypeVector %float_t 4
       %v4float_ptr_t = OpTypePointer Workgroup %v4float_t
           %modf_fn_t = OpTypeFunction %void_t %v4float_ptr_t
             %float_1 = OpConstant %float_t 1.4
           %v4float_1 = OpConstantComposite %v4float_t %float_1 %float_1 %float_1 %float_1

; All functions
        %modf_v4float = OpFunction %void_t None %modf_fn_t
             %in_iptr = OpFunctionParameter %v4float_ptr_t
; CHECK: define spir_kernel void @modf_v4float(ptr addrspace(3) %in_iptr)
                   %2 = OpLabel
                   %3 = OpExtInst %v4float_t %1 modf %v4float_1 %in_iptr
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <4 x float> @_Z4modfDv4_fPU3AS3S_(<4 x float> <float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000, float 0x3FF6666660000000>, ptr addrspace(3) %in_iptr)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
