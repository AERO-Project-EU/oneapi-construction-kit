; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_sqrt_v2float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %sqrt_v2float "sqrt_v2float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v2float_t = OpTypeVector %float_t 2
           %sqrt_fn_t = OpTypeFunction %void_t
             %float_9 = OpConstant %float_t 9.0
           %v2float_9 = OpConstantComposite %v2float_t %float_9 %float_9

; All functions
        %sqrt_v2float = OpFunction %void_t None %sqrt_fn_t
; CHECK: define spir_kernel void @sqrt_v2float()
                   %2 = OpLabel
                   %3 = OpExtInst %v2float_t %1 sqrt %v2float_9
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x float> @_Z4sqrtDv2_f(<2 x float> <float 9.000000e+00, float 9.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
