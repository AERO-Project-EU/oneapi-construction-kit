; RUN: %spirv-ll -a Vulkan -c Float64 -c Int64 %p/op_constant_null_struct.spv | %filecheck %s
            OpCapability Shader
            OpCapability Float64
            OpCapability Int64
       %1 = OpExtInstImport "GLSL.std.450"
            OpMemoryModel Logical GLSL450
            OpEntryPoint GLCompute %main "main"
            OpExecutionMode %main LocalSize 1 1 1
            OpSource GLSL 450
            OpName %main "main"
            OpName %a "a"
       %void = OpTypeVoid
          %5 = OpTypeFunction %void
        %int = OpTypeInt 32 0
        %struct = OpTypeStruct %int
    %ptr_struct = OpTypePointer Function %struct
          %8 = OpConstantNull %struct
       %main = OpFunction %void None %5
          %9 = OpLabel
          %a = OpVariable %ptr_struct Function
               OpStore %a %8
               OpReturn
               OpFunctionEnd
; CHECK: ; ModuleID = '{{.*}}'
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = type { i32 }
; CHECK: define spir_kernel void @main()
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = alloca {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK: store {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} zeroinitializer, ptr {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK: ret void
