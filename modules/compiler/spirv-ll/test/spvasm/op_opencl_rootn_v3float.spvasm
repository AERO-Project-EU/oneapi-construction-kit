; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_rootn_v3float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %rootn_v3float "rootn_v3float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %int_t = OpTypeInt 32 1
             %float_t = OpTypeFloat 32
             %v3int_t = OpTypeVector %int_t 3
           %v3float_t = OpTypeVector %float_t 3
          %rootn_fn_t = OpTypeFunction %void_t
               %int_2 = OpConstant %int_t 2
             %float_2 = OpConstant %float_t 2.0
             %v3int_2 = OpConstantComposite %v3int_t %int_2 %int_2 %int_2
           %v3float_2 = OpConstantComposite %v3float_t %float_2 %float_2 %float_2

; All functions
       %rootn_v3float = OpFunction %void_t None %rootn_fn_t
; CHECK: define spir_kernel void @rootn_v3float()
                   %2 = OpLabel
                   %3 = OpExtInst %v3float_t %1 rootn %v3float_2 %v3int_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <3 x float> @_Z5rootnDv3_fDv3_i(<3 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>, <3 x i32> <i32 2, i32 2, i32 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
