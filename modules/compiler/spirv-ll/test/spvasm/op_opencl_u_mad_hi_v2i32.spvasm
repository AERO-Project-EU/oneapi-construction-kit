; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_mad_hi_v2i32.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_mad_hi_v2i32 "u_mad_hi_v2i32"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i32_t = OpTypeInt 32 0
             %v2i32_t = OpTypeVector %i32_t 2
       %u_mad_hi_fn_t = OpTypeFunction %void_t
               %i32_2 = OpConstant %i32_t 2
               %i32_3 = OpConstant %i32_t 3
               %i32_4 = OpConstant %i32_t 4
             %v2i32_2 = OpConstantComposite %v2i32_t %i32_2 %i32_2
             %v2i32_3 = OpConstantComposite %v2i32_t %i32_3 %i32_3
             %v2i32_4 = OpConstantComposite %v2i32_t %i32_4 %i32_4

; All functions
      %u_mad_hi_v2i32 = OpFunction %void_t None %u_mad_hi_fn_t
; CHECK: define spir_kernel void @u_mad_hi_v2i32()
                   %2 = OpLabel
                   %3 = OpExtInst %v2i32_t %1 u_mad_hi %v2i32_2 %v2i32_3 %v2i32_4
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <2 x i32> @_Z6mad_hiDv2_jS_S_(<2 x i32> <i32 2, i32 2>, <2 x i32> <i32 3, i32 3>, <2 x i32> <i32 4, i32 4>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
