; RUN: %spirv-ll -a Vulkan -c Float64 -c Int64 %p/op_vector_shuffle_double.spv | %filecheck %s
            OpCapability Shader
            OpCapability Float64
            OpCapability Int64
       %1 = OpExtInstImport "GLSL.std.450"
            OpMemoryModel Logical GLSL450
            OpEntryPoint GLCompute %main "main"
            OpExecutionMode %main LocalSize 1 1 1
            OpSource GLSL 450
            OpName %main "main"
            OpName %main "main"
               OpName %res "res"
       %void = OpTypeVoid
          %3 = OpTypeFunction %void
        %double = OpTypeFloat 64
     %v3_double = OpTypeVector %double 3
 %ptr_v3_double = OpTypePointer Function %v3_double
         %10 = OpConstant %double -42.42
         %11 = OpConstantComposite %v3_double %10 %10 %10
         %13 = OpConstant %double -42.42
         %14 = OpConstantComposite %v3_double %13 %13 %13
       %main = OpFunction %void None %3
          %5 = OpLabel
        %res = OpVariable %ptr_v3_double Function
         %23 = OpVectorShuffle %v3_double %11 %14 0 3 4
               OpStore %res %23
               OpReturn
               OpFunctionEnd
; CHECK: ; ModuleID = '{{.*}}'
; CHECK: define spir_kernel void @main()
; CHECK: {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}} = alloca <3 x double>
; CHECK: store <3 x double> <double {{(-?[0-9]+\.[0-9]+e\+[0-9]+|0x[0-9A-F]+)}}, double {{(-?[0-9]+\.[0-9]+e\+[0-9]+|0x[0-9A-F]+)}}, double {{(-?[0-9]+\.[0-9]+e\+[0-9]+|0x[0-9A-F]+)}}>, ptr {{[%@][-a-zA-Z$._0-9][-a-zA-Z$._0-9]*}}
; CHECK: ret void
