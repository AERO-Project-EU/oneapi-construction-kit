; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_step_v16float.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %step_v16float "step_v16float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
          %v16float_t = OpTypeVector %float_t 16
           %step_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.9
             %float_2 = OpConstant %float_t 2.0
          %v16float_1 = OpConstantComposite %v16float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1
          %v16float_2 = OpConstantComposite %v16float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2

; All functions
       %step_v16float = OpFunction %void_t None %step_fn_t
; CHECK: define spir_kernel void @step_v16float()
                   %2 = OpLabel
                   %3 = OpExtInst %v16float_t %1 step %v16float_1 %v16float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <16 x float> @_Z4stepDv16_fS_(<16 x float> <float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000>, <16 x float> <float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00, float 2.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
