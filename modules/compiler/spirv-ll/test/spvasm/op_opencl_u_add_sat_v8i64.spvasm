; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_u_add_sat_v8i64.spv | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Int64
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %u_add_sat_v8i64 "u_add_sat_v8i64"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
               %i64_t = OpTypeInt 64 0
             %v8i64_t = OpTypeVector %i64_t 8
      %u_add_sat_fn_t = OpTypeFunction %void_t
               %i64_1 = OpConstant %i64_t 1
               %i64_2 = OpConstant %i64_t 2
             %v8i64_1 = OpConstantComposite %v8i64_t %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1 %i64_1
             %v8i64_2 = OpConstantComposite %v8i64_t %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2 %i64_2

; All functions
     %u_add_sat_v8i64 = OpFunction %void_t None %u_add_sat_fn_t
; CHECK: define spir_kernel void @u_add_sat_v8i64()
                   %2 = OpLabel
                   %3 = OpExtInst %v8i64_t %1 u_add_sat %v8i64_1 %v8i64_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x i64> @_Z7add_satDv8_mS_(<8 x i64> <i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1, i64 1>, <8 x i64> <i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2, i64 2>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
