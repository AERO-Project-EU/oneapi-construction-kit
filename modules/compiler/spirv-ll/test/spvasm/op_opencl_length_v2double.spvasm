; RUN: %spirv-ll -a OpenCL -b 64 -c Float64 %spv_file_s | %filecheck %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Float64
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %length_v2double "length_v2double"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
            %double_t = OpTypeFloat 64
          %v2double_t = OpTypeVector %double_t 2
         %length_fn_t = OpTypeFunction %void_t
            %double_3 = OpConstant %double_t 3.0
          %v2double_3 = OpConstantComposite %v2double_t %double_3 %double_3

; All functions
     %length_v2double = OpFunction %void_t None %length_fn_t
; CHECK: define spir_kernel void @length_v2double()
                   %2 = OpLabel
                   %3 = OpExtInst %double_t %1 length %v2double_3
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func double @_Z6lengthDv2_d(<2 x double> <double 3.000000e+00, double 3.000000e+00>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
