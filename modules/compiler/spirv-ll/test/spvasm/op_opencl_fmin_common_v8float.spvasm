; RUN: %spirv-ll -a OpenCL -b 64 %p/op_opencl_fmin_common_v8float.spv -o %t
; RUN: %filecheck < %t %s
                        OpCapability Kernel
                        OpCapability Addresses
                        OpCapability Vector16
                   %1 = OpExtInstImport "OpenCL.std"
                        OpMemoryModel Physical64 OpenCL
                        OpEntryPoint Kernel %fmin_common_v8float "fmin_common_v8float"
                        OpSource OpenCL_C 102000

; All types, variables and constants
              %void_t = OpTypeVoid
             %float_t = OpTypeFloat 32
           %v8float_t = OpTypeVector %float_t 8
    %fmin_common_fn_t = OpTypeFunction %void_t
             %float_1 = OpConstant %float_t 1.9
             %float_2 = OpConstant %float_t 2.4
           %v8float_1 = OpConstantComposite %v8float_t %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1 %float_1
           %v8float_2 = OpConstantComposite %v8float_t %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2 %float_2

; All functions
 %fmin_common_v8float = OpFunction %void_t None %fmin_common_fn_t
; CHECK: define spir_kernel void @fmin_common_v8float()
                   %2 = OpLabel
                   %3 = OpExtInst %v8float_t %1 fmin_common %v8float_1 %v8float_2
; CHECK: {{%[a-zA-Z0-9]+}} = call spir_func <8 x float> @_Z3minDv8_fS_(<8 x float> <float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000, float 0x3FFE666660000000>, <8 x float> <float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000, float 0x4003333340000000>)
                        OpReturn
; CHECK: ret void
                        OpFunctionEnd
; CHECK: }
